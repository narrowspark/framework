{
    "bootstrap": "vendor/autoload.php",
    "path": "benchmarks",
    "progress": "dots",
    "xml_storage_path": "build/bench",
    "retry_threshold": 5,
    "extensions": [
        "PhpBench\\Extensions\\XDebug\\XDebugExtension"
    ],
    "reports": {
        "container": {
            "title": "Time",
            "generator": "table",
            "compare": "subject",
            "compare_fields": [ "mode" ],
            "break": ["revs", "its"],
            "cols": ["benchmark"],
            "sort": { "mode": "asc" }
        }
    }
}
