        "phpstan-twig-provider": "phpstan analyse -c src/Viserio/Provider/Twig/phpstan.neon --memory-limit=-1",
        "phpstan-validation": "phpstan analyse -c src/Viserio/Component/Validation/phpstan.neon --memory-limit=-1",
        "phpstan-view": "phpstan analyse -c src/Viserio/Component/View/phpstan.neon --memory-limit=-1",
        "phpstan-web-server": "phpstan analyse -c src/Viserio/Component/WebServer/phpstan.neon --memory-limit=-1",
        "test": "phpunit",
        "test-bus": "phpunit --testsuite=\"Narrowspark Bus Component Test Suite\"",
        "test-cache": "phpunit --testsuite=\"Narrowspark Cache Component Test Suite\"",
        "test-config": "phpunit --testsuite=\"Narrowspark Config Component Test Suite\"",
        "test-console": "phpunit --testsuite=\"Narrowspark Console Component Test Suite\"",
        "test-container": "phpunit --testsuite=\"Narrowspark Container Component Test Suite\"",
        "test-contract": "phpunit --testsuite=\"Narrowspark Contract Component Test Suite\"",
        "test-cookie": "phpunit --testsuite=\"Narrowspark Cookie Component Test Suite\"",
        "test-cron": "phpunit --testsuite=\"Narrowspark Cron Component Test Suite\"",
        "test-debug-provider": "phpunit --testsuite=\"Narrowspark Debug Provider Test Suite\"",
        "test-dotenv-bridge": "phpunit --testsuite=\"Narrowspark Dotenv Bridge Test Suite\"",
        "test-events": "phpunit --testsuite=\"Narrowspark Events Component Test Suite\"",
        "test-exception": "phpunit --testsuite=\"Narrowspark Exception Component Test Suite\"",
        "test-filesystem": "phpunit --testsuite=\"Narrowspark Filesystem Component Test Suite\"",
        "test-foundation": "phpunit --testsuite=\"Narrowspark Foundation Component Test Suite\"",
        "test-http": "phpunit --testsuite=\"Narrowspark Http Component Test Suite\"",
        "test-http-factory": "phpunit --testsuite=\"Narrowspark HttpFactory Component Test Suite\"",
        "test-log": "phpunit --testsuite=\"Narrowspark Log Component Test Suite\"",
        "test-mail": "phpunit --testsuite=\"Narrowspark Mail Component Test Suite\"",
