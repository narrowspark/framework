        "phpstan-parser": "phpstan analyse -c src/Viserio/Component/Parser/phpstan.neon --memory-limit=-1",
        "phpstan-phpstan-bridge": "phpstan analyse -c src/Viserio/Bridge/Phpstan/phpstan.neon --memory-limit=-1",
        "phpstan-pipeline": "phpstan analyse -c src/Viserio/Component/Pipeline/phpstan.neon --memory-limit=-1",
        "phpstan-profiler": "phpstan analyse -c src/Viserio/Component/Profiler/phpstan.neon --memory-limit=-1",
        "phpstan-routing": "phpstan analyse -c src/Viserio/Component/Routing/phpstan.neon --memory-limit=-1",
