{
    "name": "narrowspark/framework",
    "type": "framework",
    "description": "Narrowspark Framework for Creative People.",
    "keywords": [
        "framework",
        "narrowspark",
        "viserio",
        "http",
        "request",
        "response",
        "container",
        "pipeline",
        "event",
        "translation",
        "queue"
    ],
    "homepage": "https://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
