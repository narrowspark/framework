        "test-pagination": "phpunit --testsuite=\"Narrowspark Pagination Component Test Suite\"",
        "test-parser": "phpunit --testsuite=\"Narrowspark Parser Component Test Suite\"",
        "test-phpstan-bridge": "phpunit --testsuite=\"Narrowspark Phpstan Bridge Test Suite\"",
        "test-pipeline": "phpunit --testsuite=\"Narrowspark Pipeline Component Test Suite\"",
        "test-profiler": "phpunit --testsuite=\"Narrowspark Profiler Component Test Suite\"",
        "test-routing": "phpunit --testsuite=\"Narrowspark Routing Component Test Suite\"",
        "test-session": "phpunit --testsuite=\"Narrowspark Session Component Test Suite\"",
        "test-support": "phpunit --testsuite=\"Narrowspark Support Component Test Suite\"",
