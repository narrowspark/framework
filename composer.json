        "voku/stringy": "^5.1.0"
    },
    "replace": {
        "viserio/bus": "self.version",
        "viserio/cache": "self.version",
        "viserio/config": "self.version",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/contract": "self.version",
        "viserio/cookie": "self.version",
        "viserio/cron": "self.version",
        "viserio/debug-provider": "self.version",
        "viserio/dotenv-bridge": "self.version",
        "viserio/events": "self.version",
        "viserio/exception": "self.version",
        "viserio/filesystem": "self.version",
        "viserio/foundation": "self.version",
        "viserio/http": "self.version",
        "viserio/http-factory": "self.version",
        "viserio/http-foundation": "self.version",
        "viserio/log": "self.version",
        "viserio/mail": "self.version",
        "viserio/manager": "self.version",
        "viserio/monolog-bridge": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/pagination": "self.version",
        "viserio/parser": "self.version",
        "viserio/pipeline": "self.version",
        "viserio/profiler": "self.version",
        "viserio/routing": "self.version",
        "viserio/session": "self.version",
        "viserio/support": "self.version",
        "viserio/translation": "self.version",
        "viserio/twig-bridge": "self.version",
        "viserio/twig-provider": "self.version",
        "viserio/validation": "self.version",
        "viserio/view": "self.version",
        "viserio/web-server": "self.version"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.108.0",
        "cache/filesystem-adapter": "^1.0.0",
        "cache/namespaced-cache": "^1.0.0",
        "cache/session-handler": "^1.0.0",
        "doctrine/dbal": "^2.5.0",
        "enqueue/dbal": "^0.8.0",
        "enqueue/fs": "^0.8.0",
        "enqueue/pheanstalk": "^0.8.0",
        "enqueue/redis": "^0.8.0",
        "enqueue/test": "^0.8.0",
        "erusev/parsedown": "^1.7.3",
        "erusev/parsedown-extra": "^0.7.1",
        "filp/whoops": "^2.2.0",
        "guzzlehttp/guzzle": "^6.3.0",
        "league/flysystem-aws-s3-v3": "^1.0.19",
        "league/flysystem-cached-adapter": "^1.0.6",
        "league/flysystem-sftp": "^1.0.0",
        "league/flysystem-vfs": "^1.0.0",
        "league/flysystem-webdav": "^1.0.0",
        "league/flysystem-ziparchive": "^1.0.0",
        "mikey179/vfsstream": "^1.6.7",
        "mockery/mockery": "^1.2.4",
        "mouf/picotainer": "^1.1.0",
        "narrowspark/automatic-common": "^0.9.0",
        "narrowspark/coding-standard": "^3.2.0",
        "narrowspark/testing-helper": "^8.0.1",
        "nikic/php-parser": "^4.2.1",
        "nyholm/nsa": "^1.1.0",
        "ocramius/proxy-manager": "^2.2.3",
        "pda/pheanstalk": "^3.1.0",
        "php-amqplib/php-amqplib": "^2.6.0",
        "phpstan/phpstan-php-parser": "^0.11.1",
        "phpunit/phpunit": "^8.4.2",
        "predis/predis": "^1.0.0",
        "roave/security-advisories": "dev-master",
        "spatie/flysystem-dropbox": "^1.1.0",
        "symfony/var-dumper": "^4.3.5 || ^5.0.0",
        "symfony/yaml": "^4.3.5 || ^5.0.0",
        "twig/twig": "^2.11.0 || ^3.0.0",
        "yosymfony/toml": "^1.0.4",
        "zendframework/zend-code": "^3.3.2",
        "maglnet/composer-require-checker": "^2.0.0",
        "slam/phpstan-extensions": "^3.6.1"
    },
    "config": {
        "process-timeout": 2000
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "automatic": {
            "require": {
                "symfony/symfony": "4.3.*"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Bridge\\": "src/Viserio/Bridge/",
            "Viserio\\Component\\": "src/Viserio/Component/",
            "Viserio\\Contract\\": "src/Viserio/Contract/",
            "Viserio\\Provider\\": "src/Viserio/Provider/"
        },
        "files": [
            "src/Viserio/Bridge/Dotenv/helper.php",
            "src/Viserio/Component/Container/helper.php",
            "src/Viserio/Component/Support/helper.php"
        ],
        "exclude-from-classmap": [
            "src/Viserio/Bridge/**/Tests/",
            "src/Viserio/Component/**/Tests/",
            "src/Viserio/Contract/**/Tests/",
            "src/Viserio/Provider/**/Tests/",
            "benchmarks/"
        ]
    },
    "autoload-dev": {
        "files": [
            "src/Viserio/Component/Container/Tests/Fixture/TestFunctions.php",
            "src/Viserio/Component/Container/Tests/OverwritePhpFunctions.php",
            "src/Viserio/Component/Foundation/Tests/OverwritePhpFunctions.php",
            "src/Viserio/Component/View/Tests/OverwritePhpFunctions.php",
            "src/Viserio/Component/WebServer/Tests/OverwritePhpFunctions.php"
        ]
    },
    "minimum-stability": "dev",
    "scripts": {
        "changelog": "./build/changelog.sh",
        "coverage": "phpunit --coverage-html=\"build/logs\"",
        "cs": "php-cs-fixer fix -v",
        "cs-check": "php-cs-fixer fix -v --dry-run --stop-on-violation",
        "phpstan": "phpstan analyse -c ./phpstan.neon --memory-limit=-1",
        "phpstan-bus": "phpstan analyse -c src/Viserio/Component/Bus/phpstan.neon --memory-limit=-1",
        "phpstan-cache": "phpstan analyse -c src/Viserio/Component/Cache/phpstan.neon --memory-limit=-1",
        "phpstan-config": "phpstan analyse -c src/Viserio/Component/Config/phpstan.neon --memory-limit=-1",
        "phpstan-console": "phpstan analyse -c src/Viserio/Component/Console/phpstan.neon --memory-limit=-1",
        "phpstan-container": "phpstan analyse -c src/Viserio/Component/Container/phpstan.neon --memory-limit=-1",
        "phpstan-contract": "phpstan analyse -c src/Viserio/Contract/phpstan.neon --memory-limit=-1",
        "phpstan-cookie": "phpstan analyse -c src/Viserio/Component/Cookie/phpstan.neon --memory-limit=-1",
