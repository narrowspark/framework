        "doctrine/inflector": "^1.3.0",
        "dragonmantank/cron-expression": "^2.1.0",
        "egulias/email-validator": "^2.1.0",
        "enqueue/null": "^0.8.0",
        "league/flysystem": "^1.0.55",
        "league/uri-schemes": "^1.2",
        "monolog/monolog": "^2.0.0",
        "narrowspark/arr": "^2.1.0",
        "narrowspark/collection": "^0.1.0",
        "narrowspark/http-message-util": "^0.3.0",
        "narrowspark/http-status": "^4.1.0",
        "narrowspark/mimetypes": "^1.4.0",
        "opis/closure": "^3.2.0",
        "paragonie/halite": "^4.6.0",
        "php-di/invoker": "^2.0.0",
