        "phpstan-events": "phpstan analyse -c src/Viserio/Component/Events/phpstan.neon --memory-limit=-1",
        "phpstan-exception": "phpstan analyse -c src/Viserio/Component/Exception/phpstan.neon --memory-limit=-1",
