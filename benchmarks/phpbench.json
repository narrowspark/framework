{
    "bootstrap": "vendor/autoload.php",
    "path": "src",
    "retry_threshold": 5,
    "extensions": [
        "PhpBench\\Extensions\\XDebug\\XDebugExtension"
    ],
    "progress": "dots",
    "xml_storage_path": "build/bench",
    "reports": {
        "table": {
            "generator": "table",
            "cols": [
                "benchmark",
                "subject",
                "mem_peak",
                "mean",
                "best",
                "diff"
            ],
            "sort": {
                "benchmark": "asc",
                "mean": "desc"
            }
        },
        "http": {
            "generator": "table",
            "cols": [
                "benchmark",
                "subject",
                "groups",
                "revs",
                "its",
                "mem_peak",
                "mean",
                "worst",
                "best",
                "diff"
            ],
            "sort": {
                "best": "asc",
                "groups": "asc"
            }
        }
    }
}
