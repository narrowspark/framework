{
    "name": "viserio/debug-provider",
    "type": "viserio-provider",
    "description": "The Viserio Debug provider.",
    "keywords": [
        "narrowspark",
        "viserio",
        "debug",
        "symfony",
        "provider"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/var-dumper": "^4.3.5 || ^5.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/filesystem": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/phpstan-bridge": "self.version",
        "viserio/twig-bridge": "self.version",
        "viserio/twig-provider": "self.version"
    },
    "suggest": {
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/options-resolver": "Required to use service-provider (self.version).",
        "viserio/twig-bridge": "Required to use the twig debug extension (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Provider\\Debug\\": ""
        },
        "files": [
            "helper.php"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Provider\\Debug\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
