{
    "configurators": {
        "providers": {
            "Viserio\\Provider\\Twig\\Provider\\ConsoleCommandsServiceProvider": [
                "global"
            ],
            "Viserio\\Provider\\Twig\\Provider\\TwigServiceProvider": [
                "global"
            ]
        },
        "proxies": {
            "Viserio\\Bridge\\Twig\\Proxy\\Twig": [
                "global"
            ]
        },
        "options": {
            "viserio": {
                "view": {
                    "engines": {
                        "twig": {
                            "options": {
                                "debug": "%env:APP_DEBUG%"
                            }
                        }
                    },
                    "paths": [
                        "%directory:resources%/views"
                    ]
                }
            }
        }
    }
}
