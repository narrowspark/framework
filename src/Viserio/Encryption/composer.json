{
    "name"       : "viserio/encrypter",
    "type"       : "library",
    "description": "The Viserio Encrypter package.",
    "keywords"   : ["narrowspark", "viserio", "encrypter", "openssl"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/encrypter",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                              : "^5.6 || ^7.0",
        "ext-openssl"                      : "*",
        "viserio/config"                   : "self.version",
        "viserio/contracts"                : "self.version",
        "paragonie/constant_time_encoding" : "^1 || ^2",
        "ircmaxell/random-lib"             : "^1.1"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config"  : "^1.1",
        "narrowspark/testing-helper"       : "^1.5",
        "phpunit/phpunit"                  : "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Encrypter\\"         : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Encrypter\\Tests\\"  : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                   : "1.0-dev"
        }
    },
    "suggest": {
    },
    "minimum-stability"                    : "dev",
    "prefer-stable"                        : true
}
