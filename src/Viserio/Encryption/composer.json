{
    "name"       : "viserio/encrypter",
    "type"       : "library",
    "description": "The Viserio Encrypter package.",
    "keywords"   : ["narrowspark", "viserio", "encrypter", "security", "encryption", "AES", "openssl", "cipher", "cryptography", "symmetric key cryptography", "crypto", "encrypt", "authenticated encryption"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/encrypter",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                              : "^7.0",
        "defuse/php-encryption"            : "^2.0",
        "ext-openssl"                      : "*",
        "viserio/contracts"                : "self.version"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config"  : "^1.1",
        "narrowspark/testing-helper"       : "^1.5",
        "phpunit/phpunit"                  : "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Encrypter\\"         : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Encrypter\\Tests\\"  : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                   : "1.0-dev"
        }
    },
    "suggest": {
    },
    "minimum-stability"                    : "dev",
    "prefer-stable"                        : true
}
