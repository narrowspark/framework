{
    "name": "viserio/dotenv-bridge",
    "type": "viserio-bridge",
    "description": "The Viserio Dotenv bridge.",
    "keywords": [
        "narrowspark",
        "viserio",
        "Dotenv",
        "env",
        "bridge",
        "thread-safe"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "vlucas/phpdotenv": "^3.3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "nyholm/nsa": "^1.1.0",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Bridge\\Dotenv\\": ""
        },
        "files": [
            "helper.php"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Bridge\\Dotenv\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
