{
    "name": "viserio/twig-bridge",
    "type": "viserio-bridge",
    "description": "The Viserio Twig bridge.",
    "keywords": [
        "narrowspark",
        "viserio",
        "twig",
        "bridge"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "twig/twig": "^2.4.4"
    },
    "require-dev": {
        "container-interop/service-provider": "^0.4",
        "danielstjules/stringy": "^3.0",
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^7.0.0",
        "phpunit/phpunit": "^7.2.0",
        "viserio/config": "self.version",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/profiler": "self.version",
        "viserio/session": "self.version",
        "viserio/support": "self.version",
        "viserio/translation": "self.version"
    },
    "suggest": {
        "container-interop/service-provider": "Required to use the service-provider (^0.4).",
        "viserio/config": "Required to use config extension (self.version).",
        "viserio/console": "Required to use lint and debug command (self.version).",
        "viserio/options-resolver": "Required to use the service-provider (self.version).",
        "viserio/profiler": "Required to use data collector (self.version).",
        "viserio/session": "Required to use session extension (self.version).",
        "viserio/support": "Required to use str extension (self.version).",
        "viserio/translation": "Required to use translation extension (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "automatic": {
            "providers": {
                "Viserio\\Bridge\\Twig\\Provider\\ConsoleCommandsServiceProvider": [
                    "global"
                ],
                "Viserio\\Bridge\\Twig\\Provider\\TwigBridgeDataCollectorsServiceProvider": [
                    "global"
                ],
                "Viserio\\Bridge\\Twig\\Provider\\TwigBridgeServiceProvider": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Bridge\\Twig\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Bridge\\Twig\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
