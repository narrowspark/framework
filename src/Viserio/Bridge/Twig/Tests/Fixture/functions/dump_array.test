--TEST--
"dump" function, xdebug is not loaded
--CONDITION--
!extension_loaded('xdebug')
--TEMPLATE--
{{ dump() }}
--DATA--
return array('foo' => 'foo', 'bar' => 'bar')
--CONFIG--
return array('debug' => true, 'autoescape' => false);
--EXPECT--
array(3) {
  ["foo"]=>
  string(3) "foo"
  ["bar"]=>
  string(3) "bar"
  ["global"]=>
  string(6) "global"
}
