{
    "name"       : "viserio/log",
    "type"       : "library",
    "description": "The Viserio Log package.",
    "keywords"   : ["narrowspark", "viserio", "monolog", "psr", "log"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/log",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : ">=5.6.4",
        "monolog/monolog"                 : "~1.17",
        "psr/log"                         : "~1.0",
        "viserio/cotracts"                : "self.version"
    },
    "require-dev": {
        "fabpot/php-cs-fixer"             : "dev-master",
        "narrowspark/php-cs-fixer-config" : "~1.1.4",
        "phpunit/phpunit"                 : "~5.1.3"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Log\\"              : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Log\\Tests\\"       : "Tests/"
        }
    },
    "provide": {
        "psr/log-implementation"          : "~1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "0.10.0-dev"
        }
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
