{
    "name"       : "viserio/view",
    "type"       : "library",
    "description": "The Viserio View package.",
    "keywords"   : ["narrowspark", "viserio", "view", "php", "virtuoso"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/api",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                                : "^5.6 || ^7.0",
        "container-interop/container-interop"                : "^1.1",
        "symfony/event-dispatcher"                           : "^3.0 || ^2.8",
        "viserio/contracts"                                  : "self.version",
        "viserio/filesystem"                                 : "self.version",
        "viserio/support"                                    : "self.version"
    },
    "require-dev": {
        "mockery/mockery"                                    : "^0.9",
        "narrowspark/php-cs-fixer-config"                    : "^1.1",
        "narrowspark/testing-helper"                         : "^1.5",
        "phpunit/phpunit"                                    : "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\View\\"                                : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\View\\Tests\\"                         : "Tests/"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation" : "~1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                                     : "1.0-dev"
        }
    },
    "minimum-stability"                                      : "dev",
    "prefer-stable"                                          : true
}
