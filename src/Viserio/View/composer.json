{
    "name"       : "viserio/view",
    "type"       : "library",
    "description": "The Viserio View package.",
    "keywords"   : ["narrowspark", "viserio", "view", "php"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/api",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                                : ">=5.6.4",
        "container-interop/container-interop"                : "~1.1",
        "viserio/contracts"                                  : "self.version",
        "viserio/events"                                     : "self.version",
        "viserio/filesystem"                                 : "self.version",
        "viserio/support"                                    : "self.version"
    },
    "require-dev": {
        "fabpot/php-cs-fixer"                                : "dev-master",
        "mockery/mockery"                                    : "~0.9.4",
        "narrowspark/php-cs-fixer-config"                    : "~1.1.4",
        "phpunit/phpunit"                                    : "~5.1.3",
        "symfony/event-dispatcher"                           : "~2.8.*|~3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\View\\"                                : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\View\\Tests\\"                         : "tests/"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation" : "~1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                                     : "0.10.0-dev"
        }
    },
    "minimum-stability"                                      : "dev",
    "prefer-stable"                                          : true
}
