{
    "name": "viserio/contract",
    "type": "library",
    "description": "The Viserio Contract package.",
    "keywords": [
        "framework",
        "narrowspark",
        "contract",
        "interfaces",
        "message",
        "psr-7",
        "log",
        "psr-3",
        "RFC 2616",
        "RFC"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "replace": {
        "viserio/bus-contract": "self.version",
        "viserio/cache-contract": "self.version",
        "viserio/config-contract": "self.version",
        "viserio/console-contract": "self.version",
        "viserio/container-contract": "self.version",
        "viserio/cookie-contract": "self.version",
        "viserio/cron-contract": "self.version",
        "viserio/events-contract": "self.version",
        "viserio/exception-contract": "self.version",
        "viserio/filesystem-contract": "self.version",
        "viserio/foundation-contract": "self.version",
        "viserio/http-contract": "self.version",
        "viserio/http-factory-contract": "self.version",
        "viserio/http-foundation-contract": "self.version",
        "viserio/log-contract": "self.version",
        "viserio/mail-contract": "self.version",
        "viserio/manager-contract": "self.version",
        "viserio/options-resolver-contract": "self.version",
        "viserio/pagination-contract": "self.version",
        "viserio/parser-contract": "self.version",
        "viserio/pipeline-contract": "self.version",
        "viserio/profiler-contract": "self.version",
        "viserio/routing-contract": "self.version",
        "viserio/session-contract": "self.version",
        "viserio/support-contract": "self.version",
        "viserio/translation-contract": "self.version",
        "viserio/validation-contract": "self.version",
        "viserio/view-contract": "self.version",
        "viserio/web-server-contract": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/log-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.4",
        "narrowspark/testing-helper": "^8.0.2",
        "narrowspark/coding-standard": "^3.1.0",
        "slam/phpstan-extensions": "^3.6.1",
        "phpunit/phpunit": "8.2.*",
        "psr/cache": "^1.0.1",
        "psr/container": "^1.0.0",
        "psr/http-message": "^1.0.0",
        "psr/http-server-middleware": "^1.0.0",
        "symfony/console": "^4.3.8 || ^5.0.0"
    },
    "suggest": {
        "psr/http-message": "Required to use Http, Routing contract (^1.0.0).",
        "psr/http-server-middleware": "Required to use Routing Contract Middleware Contracts (^1.0.0).",
        "symfony/console": "Required to use the Console Contract (^4.3.8 | ^5.0.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Contract\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Contract\\Tests\\": "Tests"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
