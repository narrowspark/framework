{
    "name"       : "viserio/http",
    "type"       : "library",
    "description": "The Viserio Http package.",
    "keywords"   : ["narrowspark", "viserio", "http", "request", "response", "stack", "kernel"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/http",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                       : "^5.6 || ^7.0",
        "viserio/container"         : "self.version",
        "viserio/cookie"            : "self.version",
        "viserio/contracts"         : "self.version",
        "viserio/session"           : "self.version",
        "stack/builder"             : "^1.0.3",
        "symfony/http-foundation"   : "^3.0 || ^2.8",
        "symfony/http-kernel"       : "^3.0 || ^2.8"
    },
    "require-dev": {
        "fabpot/php-cs-fixer"       : "dev-master",
        "phpunit/phpunit"           : "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Http\\"       : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Http\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"            : "1.0-dev"
        }
    },
    "minimum-stability"             : "dev",
    "prefer-stable"                 : true
}
