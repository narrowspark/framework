{
    "name"       : "viserio/hashing",
    "type"       : "library",
    "description": "The Viserio Hashing package.",
    "keywords"   : ["narrowspark", "viserio", "hashing", "password"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/hashing",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                          : ">=5.6.4",
        "viserio/support"              : "self.version",
        "viserio/contracts"            : "self.version",
        "paragonie/password_lock"      : "~1.1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer"          : "dev-master",
        "phpunit/phpunit"              : "~5.0.6"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Hashing\\"       : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Hashing\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"               : "0.10.0-dev"
        }
    },
    "minimum-stability"                : "dev",
    "prefer-stable"                    : true
}
