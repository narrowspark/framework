{
    "name"       : "viserio/hashing",
    "type"       : "library",
    "description": "The Viserio Hashing package.",
    "keywords"   : ["narrowspark", "viserio", "hashing", "password", "hash"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/hashing",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : "^7.0.7",
        "defuse/php-encryption"           : "^2.0",
        "paragonie/password_lock"         : "^2.0 || ^3.0",
        "viserio/contracts"               : "self.version"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config" : "^1.1",
        "narrowspark/testing-helper"      : "^1.5",
        "phpunit/phpunit"                 : "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Hashing\\"          : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Hashing\\Tests\\"   : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
