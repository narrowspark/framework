{
    "name"       : "viserio/support",
    "type"       : "library",
    "description": "The Viserio Support package.",
    "keywords"   : ["framework", "narrowspark", "support", "viserio", "env", "manager", "dumper"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : "7.0.0 - 7.0.5 || ^7.0.7",
        "container-interop/container-interop" : "^1.1",
        "danielstjules/stringy"               : "^2.3",
        "paragonie/constant_time_encoding"    : "^2.0",
        "viserio/contracts"                   : "self.version"
    },
    "require-dev": {
        "php-di/invoker"                      : "^1.2",
        "narrowspark/php-cs-fixer-config"     : "^1.1",
        "narrowspark/testing-helper"          : "^1.5",
        "phpunit/phpunit"                     : "^5.1",
        "symfony/var-dumper"                  : "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Support\\"              : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Support\\Tests\\"       : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "1.0-dev"
        }
    },
    "suggest": {
        "php-di/invoker"                      : "Required to use the Invoker Class (~1.2)",
        "symfony/var-dumper"                  : "Improves the Dumper::dump() (^3.1)."
    },
    "minimum-stability"                       : "dev",
    "prefer-stable"                           : true
}
