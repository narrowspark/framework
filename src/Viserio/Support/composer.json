{
    "name"       : "viserio/support",
    "type"       : "library",
    "description": "The Viserio Support package.",
    "keywords"   : ["framework", "narrowspark", "support", "viserio", "env", "manager", "dumper", "invoker"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                                : "^7.1",
        "viserio/contracts"                                  : "self.version"
    },
    "require-dev": {
        "danielstjules/stringy"                              : "^2.3",
        "narrowspark/testing-helper"                         : "^2.0",
        "mockery/mockery"                                    : "^0.9",
        "php-di/invoker"                                     : "^1.3",
        "phpunit/phpunit"                                    : "~5.7",
        "symfony/var-dumper"                                 : "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Support\\"                             : ""
        },
        "exclude-from-classmap"                              : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Support\\Tests\\"                      : "Tests/"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation" : "^1.1",
        "psr/container-implementation"                       : "^1.0"
    },
    "suggest": {
        "danielstjules/stringy"                              : "Required to use the Stringy Class in Str (^2.3)",
        "php-di/invoker"                                     : "Required to use the Invoker Class (^1.3)",
        "symfony/var-dumper"                                 : "Improves the Dumper::dump() (^3.1)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                                     : "1.0-dev"
        }
    },
    "minimum-stability"                                      : "dev",
    "prefer-stable"                                          : true
}
