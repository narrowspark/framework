{
    "name"       : "viserio/parsers",
    "type"       : "library",
    "description": "The Viserio Filesystem package.",
    "keywords"   : [
        "narrowspark",
        "viserio",
        "ini",
        "php",
        "toml",
        "yaml",
        "xml",
        "parser",
        "parsers"
    ],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/parsers",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : "^5.6|^7.0",
        "viserio/contracts"               : "self.version"
    },
    "require-dev": {
        "mikey179/vfsStream"              : "^1.6",
        "narrowspark/php-cs-fixer-config" : "^1.1",
        "narrowspark/testing-helper"      : "^1.5",
        "phpunit/phpunit"                 : "^5.1",
        "symfony/yaml"                    : "^2.8|^3.0",
        "yosymfony/toml"                  : "^0.3"
    },
    "suggest": {
        "symfony/yaml"                    : "Required to use the Yaml parser (~2.8|~3.0).",
        "yosymfony/toml"                  : "Required to use the TOML parser (^0.3)."
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Parsers\\"          : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Parsers\\Tests\\"   : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
