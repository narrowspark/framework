{
    "name"       : "viserio/parsers",
    "type"       : "library",
    "description": "The Viserio Parsers package.",
    "keywords"   : [
        "narrowspark",
        "viserio",
        "ini",
        "php",
        "toml",
        "yaml",
        "xml",
        "parser",
        "parsers"
    ],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : "7.0.0 - 7.0.5 || ^7.0.7",
        "viserio/contracts"                   : "self.version",
        "viserio/support"                     : "self.version"
    },
    "require-dev": {
        "container-interop/service-provider"  : "^0.3",
        "mikey179/vfsStream"                  : "^1.6",
        "narrowspark/testing-helper"          : "^1.5",
        "phpunit/phpunit"                     : "^5.1",
        "psr/http-message"                    : "^1.0",
        "sepia/po-parser"                     : "^4.2",
        "symfony/yaml"                        : "^3.1",
        "yosymfony/toml"                      : "^0.3",
        "viserio/container"                   : "self.version"
    },
    "provide": {
        "psr/http-message"                    : "^1.0",
        "psr/http-message-implementation"     : "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Parsers\\"              : ""
        },
        "exclude-from-classmap"               : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Parsers\\Tests\\"       : "Tests/"
        }
    },
    "suggest": {
        "container-interop/service-provider"  : "Required to use service-provider (^0.3).",
        "symfony/yaml"                        : "Required to use the yaml parser (^3.1).",
        "sepia/po-parser"                     : "Required to use the PO parser (^4.2).",
        "spatie/array-to-xml"                 : "Required to use the XML dump (^2.1.1).",
        "yosymfony/toml"                      : "Required to use the TOML parser (^0.3).",
        "vlucas/phpdotenv"                    : "Required to use the .env parser (^2.3).",
        "viserio/statical-proxy"              : "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "1.0-dev"
        }
    },
    "prefer-stable"                           : true
}
