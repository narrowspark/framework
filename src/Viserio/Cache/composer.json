{
    "name"       : "viserio/cache",
    "type"       : "library",
    "description": "The Viserio Cache package.",
    "keywords"   : [
        "narrowspark",
        "viserio",
        "apc",
        "APCu",
        "redis",
        "mongodb",
        "mongo",
        "igbinary",
        "serializer",
        "json",
        "pdo",
        "sqlite",
        "mysql",
        "postgres",
        "pgsql",
        "memcached",
        "memcache",
        "psrCache",
        "psr-6",
        "psr-cache",
        "Filesystem",
        "session",
        "cache",
        "caching"
    ],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/cache",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : ">=5.6.4",
        "cache/adapter-common"                : "~0.1.4",
        "cache/hierarchical-cache"            : "~0.1",
        "cache/taggable-cache"                : "~0.3.1",
        "container-interop/container-interop" : "~1.1",
        "psr/cache"                           : "~1.0",
        "viserio/config"                      : "self.version",
        "viserio/connect"                     : "self.version",
        "viserio/cotracts"                    : "self.version"
    },
    "require-dev": {
        "cache/integration-tests"             : "~0.5",
        "predis/predis"                       : "~1.0.1",
        "narrowspark/php-cs-fixer-config"     : "~1.1.5",
        "narrowspark/testing-helper"          : "~1.1",
        "phpunit/phpunit"                     : "~5.1.3"
    },
    "provide": {
        "psr/cache-implementation"            : "1.0.0"
    },
    "replace": {
        "psr/cache"                           : "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Cache\\"                : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Cache\\Tests\\"         : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "1.0-dev"
        }
    },
    "suggest": {
        "predis/predis"                       : "Flexible and feature-complete PHP client library for Redis",
        "ext-apc"                             : "Required to use the APC cache.",
        "ext-apcu"                            : "Required to use the APCu cache.",
        "predis/predis"                       : "Required to use the Redis cache (~1.0.1).",
        "ext-mongo"                           : "Required to use the Monvo cache.",
        "ext-memcached"                       : "Required to use the Memcahed cache.",
        "ext-pdo"                             : "Required to use the PDO cache."
    },
    "minimum-stability"                       : "dev",
    "prefer-stable"                           : true
}
