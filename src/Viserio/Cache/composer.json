{
    "name"       : "viserio/cache",
    "type"       : "library",
    "description": "The Viserio Cache package.",
    "keywords"   : ["narrowspark", "viserio", "cache", "apc", "apcu", "redis", "mongo"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/cache",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : ">=5.6.4",
        "narrowspark/arr"                 : "~1.0",
        "nesbot/carbon"                   : "~1.20",
        "viserio/container"               : "self.version",
        "viserio/config"                  : "self.version",
        "viserio/cotracts"                : "self.version"
    },
    "require-dev": {
        "predis/predis"                   : "~1.0.1",
        "narrowspark/php-cs-fixer-config" : "~1.1.5",
        "narrowspark/testing-helper"      : "~1.1",
        "phpunit/phpunit"                 : "~5.1.3"
    },
    "provide": {
        "psr/cache-implementation"   : "1.0.0"
    },
    "replace": {
        "psr/cache"                  : "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Cache\\"            : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Cache\\Tests\\"     : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "suggest": {
        "predis/predis"                   : "Flexible and feature-complete PHP client library for Redis",
        "ext-apc"                         : "*",
        "ext-apcu"                        : "*",
        "ext-redis"                       : "*",
        "ext-mongo"                       : "*",
        "ext-memcached"                   : "*",
        "ext-pdo"                         : "*",
        "ext-pdo_sqlite"                  : "*",
        "ext-pdo_mysql"                   : "*",
        "ext-pdo_pgsql"                   : "*"
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
