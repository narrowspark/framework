{
    "name"       : "viserio/queue",
    "type"       : "library",
    "description": "The Viserio Queue package.",
    "keywords"   : ["narrowspark", "viserio", "queue", "pheanstalk"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                                : "7.0.0 - 7.0.5 || ^7.0.7",
        "cakephp/chronos"                                    : "^1.0",
        "container-interop/container-interop"                : "^1.1",
        "narrowspark/arr"                                    : "^1.1",
        "opis/closure"                                       : "^2.2.1",
        "symfony/process"                                    : "^3.1",
        "viserio/cotracts"                                   : "self.version",
        "viserio/console"                                    : "self.version",
        "viserio/encryption"                                 : "self.version",
        "viserio/support"                                    : "self.version"
    },
    "require-dev": {
        "narrowspark/testing-helper"                         : "^1.5",
        "phpunit/phpunit"                                    : "^5.1",
        "php-amqplib/php-amqplib"                            : "^2.6",
        "pda/pheanstalk"                                     : "^3.1",
        "viserio/connect"                                    : "self.version",
        "viserio/events"                                     : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Queue\\"                               : ""
        },
        "exclude-from-classmap"                              : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Queue\\Tests\\"                        : "Tests/"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation" : "^1.1",
        "psr/container-implementation"                       : "^1.0"
    },
    "suggest": {
        "google/appengine-php-sdk"                           : "Required to use the Google App Engine queue driver (^1.9.30).",
        "aws/aws-sdk-php"                                    : "Required to use the SQS queue driver (^3.0).",
        "pda/pheanstalk"                                     : "Required to use the Beanstalk queue driver (^3.0).",
        "iron-io/iron_mq"                                    : "Required to use the IronMQ queue driver (^4.0).",
        "viserio/connect"                                    : "Required to use the Redis and Database queue driver (^0.4)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                                     : "1.0-dev"
        }
    },
    "minimum-stability"                                      : "dev",
    "prefer-stable"                                         : true
}
