{
    "name"       : "viserio/queue",
    "type"       : "library",
    "description": "The Viserio Queue package.",
    "keywords"   : ["narrowspark", "viserio", "queue", "symfony"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/queue",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : "^5.6 || ^7.0",
        "viserio/cotracts"                : "self.version",
        "symfony/event-dispatcher"        : "~2.8.* || ~3.0.*"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config" : "~1.1.5",
        "narrowspark/testing-helper"      : "~1.5",
        "phpunit/phpunit"                 : "~5.1.3"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Queue\\"            : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Queue\\Test\\"      : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
