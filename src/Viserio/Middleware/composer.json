{
    "name"       : "viserio/middleware",
    "type"       : "library",
    "description": "The Viserio Middleware package.",
    "keywords"   : ["framework", "narrowspark", "viserio", "middleware", "psr7", "psr-7"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                                : "^5.6 || ^7.0",
        "container-interop/container-interop"                : "~1.1",
        "psr/http-message"                                   : "~1.0",
        "viserio/cotracts"                                   : "self.version"
    },
    "require-dev": {
        "fabpot/php-cs-fixer"                                : "dev-master",
        "narrowspark/php-cs-fixer-config"                    : "~1.1.5",
        "narrowspark/testing-helper"                         : "~1.5",
        "phpunit/phpunit"                                    : "~5.1.3"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Middleware\\"                          : ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                                     : "1.0-dev"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation" : "~1.1",
        "psr/http-message-implementation"                    : "~1.0"
    },
    "minimum-stability"                                      : "dev",
    "prefer-stable"                                          : true
}
