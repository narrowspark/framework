{
    "name"       : "viserio/web-profiler",
    "type"       : "library",
    "description": "The Viserio WebProfiler package.",
    "keywords"   : ["narrowspark", "viserio", "profiler", "web-profiler"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : "7.0.0 - 7.0.5 || ^7.0.7",
        "http-interop/http-factory"           : "^0.2",
        "psr/cache"                           : "^1.0",
        "psr/http-message"                    : "^1.0",
        "symfony/var-dumper"                  : "^3.1",
        "viserio/contracts"                   : "self.version"
    },
    "require-dev": {
        "narrowspark/testing-helper"          : "^1.5",
        "phpunit/phpunit"                     : "^5.1",
        "http-interop/http-middleware"        : "^0.3",
        "viserio/cache"                       : "self.version",
        "viserio/config"                      : "self.version",
        "viserio/container"                   : "self.version",
        "viserio/routing"                     : "self.version",
        "viserio/events"                      : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\WebProfiler\\"          : ""
        },
        "exclude-from-classmap"               : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\WebProfiler\\Tests\\"   : "Tests/"
        }
    },
    "provide": {
        "psr/log-implementation"              : "^1.0",
        "psr/http-message"                    : "^1.0",
        "psr/http-message-implementation"     : "^1.0"
    },
    "suggest": {
        "container-interop/service-provider"  : "Required to use service-provider (^0.3).",
        "http-interop/http-middleware"        : "Required to use middleware (^0.3).",
        "viserio/statical-proxy"              : "Required to use the proxy instances (self.version).",
        "viserio/routing"                     : "Required to use the webprofiler assets controllers (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "1.0-dev"
        }
    },
    "minimum-stability"                       : "dev",
    "prefer-stable"                           : true
}
