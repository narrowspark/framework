{
    "name"       : "viserio/contracts",
    "type"       : "library",
    "description": "The Viserio Contracts package.",
    "keywords"   : [
        "framework",
        "narrowspark",
        "contracts",
        "interfaces",
        "message",
        "psr-7",
        "log",
        "psr-3",
        "RFC 2616",
        "RFC"
    ],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                                : "7.0.0 - 7.0.5 || ^7.0.7"
    },
    "require-dev": {
        "phpunit/phpunit"                                    : "^5.1",
        "narrowspark/testing-helper"                         : "^1.5",
        "container-interop/container-interop"                : "^1.1",
        "psr/http-message"                                   : "^1.0",
        "psr/log"                                            : "^1.0",
        "symfony/console"                                    : "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Contracts\\"                           : ""
        },
        "exclude-from-classmap"                              : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Contracts\\Tests\\"                    : "Tests/"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation" : "^1.1",
        "psr/container-implementation"                       : "^1.0",
        "psr/http-message-implementation"                    : "^1.0",
        "psr/log-implementation"                             : "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                                     : "1.0-dev"
        }
    },
    "suggest": {
        "psr/log"                                            : "Required to use log contracts (^1.0).",
        "psr/http-message"                                   : "Required to use Http, Middleware, Routing contracts (^1.0).",
        "container-interop/container-interop"                : "Required to use Container contract (^1.1).",
        "container-interop/service-provider"                 : "Required to use Container contract (^0.3)."
    },
    "minimum-stability"                                      : "dev",
    "prefer-stable"                                          : true
}
