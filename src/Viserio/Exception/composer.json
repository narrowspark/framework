{
    "name"       : "viserio/exception",
    "type"       : "library",
    "description": "The Viserio Exception package.",
    "keywords"   : ["narrowspark", "viserio", "exception", "debug", "http", "psr7", "middleware"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : "7.0.0 - 7.0.5 || ^7.0.7",
        "ramsey/uuid"                         : "^3.4",
        "viserio/config"                      : "self.version",
        "viserio/cotracts"                    : "self.version",
        "viserio/http"                        : "self.version"
    },
    "require-dev": {
        "filp/whoops"                         : "^2.0",
        "narrowspark/php-cs-fixer-config"     : "^1.1",
        "narrowspark/testing-helper"          : "^1.5",
        "phpunit/phpunit"                     : "^5.1",
        "viserio/view"                        : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Exception\\"            : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Exception\\Tests\\"     : "Tests/"
        }
    },
    "suggest": {
        "filp/whoops"                         : "Enables use of the whoops displayer (^2.0).",
        "narrowspark/view"                    : "Enables use of the view displayer (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "1.0-dev"
        }
    },
    "minimum-stability"                       : "dev",
    "prefer-stable"                           : true
}
