{
    "name": "viserio/profiler",
    "type": "library",
    "description": "The Viserio Profiler package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "profiler",
        "web-profiler",
        "debug"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "narrowspark/http-message-util": "^0.3.0",
        "psr/cache": "^1.0.1",
        "psr/http-factory": "^1.0.0",
        "psr/http-message": "^1.0",
        "symfony/stopwatch": "^4.3.5 || ^5.0.0",
        "symfony/var-dumper": "^4.3.5 || ^5.0.0",
        "viserio/cache-contract": "^1.0@dev",
        "viserio/events-contract": "^1.0@dev",
        "viserio/http-factory-contract": "^1.0@dev",
        "viserio/profiler-contract": "^1.0@dev",
        "viserio/routing-contract": "^1.0@dev",
        "viserio/session-contract": "^1.0@dev",
        "viserio/support": "^1.0@dev"
    },
    "provide": {
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/log-implementation": "^1.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "psr/http-server-middleware": "^1.0",
        "slam/phpstan-extensions": "^3.6.1",
        "swiftmailer/swiftmailer": "^6.0.0",
        "viserio/cache": "^1.0@dev",
        "viserio/container": "^1.0@dev",
        "viserio/events": "^1.0@dev",
        "viserio/http-factory": "^1.0@dev",
        "viserio/options-resolver": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev",
        "viserio/routing": "^1.0@dev"
    },
    "suggest": {
        "psr/http-server-middleware": "Required to use middleware (^1.0).",
        "viserio/container": "Required to use service-provider (^1.0@dev).",
        "viserio/options-resolver": "Required to use service-provider (^1.0@dev).",
        "viserio/routing": "Required to use the Profiler assets controllers (^1.0@dev)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Profiler\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Profiler\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
