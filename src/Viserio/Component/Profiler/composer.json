{
    "name": "viserio/profiler",
    "type": "library",
    "description": "The Viserio Profiler package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "profiler",
        "web-profiler",
        "debug"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "narrowspark/http-message-util": "^0.3.0",
        "psr/cache": "^1.0.1",
        "psr/http-factory": "^1.0.0",
        "psr/http-message": "^1.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "symfony/stopwatch": "^4.1.0",
        "symfony/var-dumper": "^4.1.3",
        "viserio/contract": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/log-implementation": "^1.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "cache/array-adapter": "^0.4",
        "mockery/mockery": "^1.2.0",
        "narrowspark/testing-helper": "^7.0.0",
        "phpunit/phpunit": "^7.2.0",
        "psr/http-server-middleware": "^1.0",
        "viserio/cache": "self.version",
        "viserio/container": "self.version",
        "viserio/events": "self.version",
        "viserio/http-factory": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/routing": "self.version"
    },
    "suggest": {
        "container-interop/service-provider": "Required to use service-provider (^0.4).",
        "psr/http-server-middleware": "Required to use middleware (^1.0).",
        "viserio/options-resolver": "Required to use service-provider (self.version).",
        "viserio/routing": "Required to use the Profiler assets controllers (self.version).",
        "viserio/statical-proxy": "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Profiler\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Profiler\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
