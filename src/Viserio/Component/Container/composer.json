{
    "name": "viserio/container",
    "type": "library",
    "description": "The Viserio Container package.",
    "keywords": [
        "framework",
        "narrowspark",
        "viserio",
        "container",
        "container-interop",
        "di",
        "dependency",
        "injection",
        "dependency injection"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "container-interop/service-provider": "^0.4",
        "php-di/invoker": "^2.0",
        "psr/container": "^1.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/contract": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "mouf/picotainer": "^1.1.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Container\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Container\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
