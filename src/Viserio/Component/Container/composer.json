{
    "name": "viserio/container",
    "type": "library",
    "description": "The Viserio Container package.",
    "keywords": [
        "framework",
        "narrowspark",
        "viserio",
        "container",
        "di",
        "dependency",
        "injection",
        "dependency injection"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-reflection": "*",
        "php-di/invoker": "^2.0.0",
        "psr/container": "^1.0",
        "roave/better-reflection": "^3.1.1",
        "opis/closure": "^3.0.12",
        "symfony/polyfill-mbstring": "^1.8.0",
        "symfony/var-exporter": "^4.2.0",
        "viserio/contract": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.0",
        "narrowspark/testing-helper": "^7.0.0",
        "nyholm/nsa": "^1.1",
        "ocramius/proxy-manager": "^2.2.1",
        "phpunit/phpunit": "^7.2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Container\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ],
        "files": [
            "helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Container\\Tests\\": "Tests/"
        },
        "files": [
            "Tests/Fixture/testFunctions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
