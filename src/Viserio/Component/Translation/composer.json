{
    "name"       : "viserio/translation",
    "type"       : "library",
    "description": "The Viserio Translator package.",
    "keywords"   : ["narrowspark", "viserio", "translator", "translation", "trans", "languages", "lang", "localisation", "localization", "validations"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                : "^7.1",
        "viserio/contracts"                  : "self.version",
        "viserio/filesystem"                 : "self.version"
    },
    "require-dev": {
        "container-interop/service-provider" : "^0.3",
        "mikey179/vfsStream"                 : "^1.6",
        "narrowspark/testing-helper"         : "^2.0",
        "phpunit/phpunit"                    : "^6.0",
        "viserio/container"                  : "self.version",
        "viserio/log"                        : "self.version",
        "viserio/parsers"                    : "self.version",
        "viserio/options-resolver"           : "self.version",
        "viserio/web-profiler"               : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Translation\\"         : ""
        },
        "exclude-from-classmap"              : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            " Viserio\\Translation\\Tests\\" : "Tests/"
        }
    },
    "suggest": {
        "container-interop/service-provider" : "Required to use service-provider (^0.3).",
        "viserio/parsers"                    : "Required to use service-provider or the import function (self.version).",
        "viserio/options-resolver"           : "Required to use service-provider (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                     : "1.0-dev"
        }
    },
    "minimum-stability"                      : "dev",
    "prefer-stable"                          : true
}
