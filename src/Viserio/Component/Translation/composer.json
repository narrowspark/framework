{
    "name": "viserio/translation",
    "type": "library",
    "description": "The Viserio Translator package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "translator",
        "translation",
        "trans",
        "languages",
        "lang",
        "localization",
        "validations"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-tokenizer": "*",
        "psr/log": "^1.0.2",
        "symfony/polyfill-intl-icu": "^1.8.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/parser": "self.version",
        "viserio/translation-contract": "self.version"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.7",
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/log": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/phpstan-bridge": "self.version",
        "viserio/profiler": "self.version"
    },
    "suggest": {
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/options-resolver": "Required to use service-provider (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Translation\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Translation\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
