{
    "name": "viserio/foundation",
    "type": "library",
    "description": "The Viserio Foundation package.",
    "keywords": [
        "narrowspark",
        "sparkle",
        "viserio",
        "foundation",
        "container",
        "config",
        "log",
        "exception",
        "parser",
        "console"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-spl": "*",
        "psr/container": "^1.0",
        "symfony/filesystem": "^4.3.5 || ^5.0.0",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/console": "self.version",
        "viserio/container-contract": "self.version",
        "viserio/dotenv-bridge": "self.version",
        "viserio/exception-contract": "self.version",
        "viserio/foundation-contract": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "symfony/var-exporter": "^4.3.5 || ^5.0.0",
        "viserio/config": "self.version",
        "viserio/container": "self.version",
        "viserio/exception": "self.version",
        "viserio/http-factory": "self.version",
        "viserio/phpstan-bridge": "self.version",
        "viserio/profiler": "self.version"
    },
    "suggest": {
        "psr/http-factory": "Required to use the SetRequestForConsole (^1.0.0).",
        "viserio/config": "Required to use the LoadConfiguration bootstrap class (self.version).",
        "viserio/exception": "Required for a better http and console exception output (self.version).",
        "viserio/container": "Required to use the ConfigureKernelBootstrap and SyntheticInstancesBootstrap (self.version)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Foundation\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Foundation\\Tests\\": "Tests/"
        },
        "files": [
            "Tests/OverwritePhpFunctions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
