{
    "name": "viserio/foundation",
    "type": "library",
    "description": "The Viserio Foundation package.",
    "keywords": [
        "narrowspark",
        "sparkle",
        "viserio",
        "foundation",
        "container",
        "config",
        "log",
        "exception",
        "events",
        "parser",
        "routing"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "ext-spl": "*",
        "psr/container": "^1.0",
        "viserio/container": "self.version",
        "viserio/contract": "self.version",
        "viserio/exception": "self.version",
        "viserio/log": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "cakephp/chronos": "^1.0.4",
        "composer/composer": "^1.6",
        "http-interop/http-factory": "^0.3",
        "mockery/mockery": "^1.0",
        "narrowspark/discovery-common": "^0.8.0",
        "narrowspark/http-status": "^4.0",
        "narrowspark/testing-helper": "^6.0",
        "paragonie/halite": "^4.4.2",
        "phpunit/phpunit": "^7.0",
        "symfony/filesystem": "^4.0.6",
        "symfony/process": "^4.0.6",
        "viserio/config": "self.version",
        "viserio/console": "self.version",
        "viserio/cron": "self.version",
        "viserio/events": "self.version",
        "viserio/pipeline": "self.version"
    },
    "suggest": {
        "cakephp/chronos": "Required to use CheckForMaintenanceModeMiddleware and DownCommand (^1.0.4)",
        "http-interop/http-factory": "Required to use the SetRequestForConsole (^0.3).",
        "narrowspark/http-status": "Required to use the Down-, UpCommand and MaintenanceModeException (^4.0).",
        "symfony/process": "Required to use the Serve Command (^4.0.6).",
        "viserio/config": "Required to use the LoadConfiguration bootstrap class (self.version).",
        "viserio/console": "Required to use the Console Kernel (self.version).",
        "viserio/cron": "Required to use the getSchedule function in the Console kernel (self.version).",
        "viserio/log": "Required for adding logging functionality to the framework (self.version).",
        "viserio/pipeline": "Required to use psr-15 middleware (self.version).",
        "viserio/routing": "Required to use the Http Kernel (self.version).",
        "viserio/statical-proxy": "Required to use statical-proxy bootstrap class (self.version).",
        "vlucas/phpdotenv": "Required to use the LoadEnvironmentVariables bootstrap class (^2.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "custom-configurators": {
                "project": "Viserio\\Component\\Foundation\\Discovery\\Configurator\\ProjectConfigurator"
            },
            "env": {
                "APP_ENV": "prod"
            },
            "project": "true",
            "providers": {
                "Viserio\\Component\\Foundation\\Provider\\ConsoleCommandsServiceProvider": [
                    "global"
                ],
                "Viserio\\Component\\Foundation\\Provider\\FoundationDataCollectorServiceProvider": [
                    "local",
                    "testing"
                ]
            },
            "proxies": {
                "Viserio\\Component\\Foundation\\Proxy\\App": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Foundation\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Foundation\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
