{
    "type": "project",
    "keywords": [
        "framework",
        "narrowspark",
        "viserio"
    ],
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "roave/security-advisories": "dev-master",
        "narrowspark/discovery": "dev-master",
        "viserio/foundation": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "phpunit/phpunit": "^7.2.0",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "narrowspark/framework": "*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "discovery": {
            "allow-auto-install": true
        },
        "discovery_test" : "true",
        "app-dir": "src/Viserio/Component/Foundation/Tests/ComposerScriptsTest/app",
        "config-dir": "src/Viserio/Component/Foundation/Tests/ComposerScriptsTest/config",
        "public-dir": "src/Viserio/Component/Foundation/Tests/ComposerScriptsTest/public",
        "resources-dir": "src/Viserio/Component/Foundation/Tests/ComposerScriptsTest/resources",
        "routes-dir": "src/Viserio/Component/Foundation/Tests/ComposerScriptsTest/routes",
        "tests-dir": "src/Viserio/Component/Foundation/Tests/ComposerScriptsTest/tests",
        "storage-dir": "src/Viserio/Component/Foundation/Tests/ComposerScriptsTest/storage"
    }
}
