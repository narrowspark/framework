{
    "name": "viserio/exception",
    "type": "library",
    "description": "The Viserio Exception package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "exception",
        "debug",
        "http",
        "psr7",
        "middleware"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "narrowspark/http-message-util": "^0.3.0",
        "narrowspark/http-status": "^4.1.0",
        "psr/http-factory": "^1.0.0",
        "psr/log": "^1.0.2",
        "symfony/debug": "^4.3.5 || ^5.0.0",
        "viserio/exception-contract": "self.version",
        "viserio/http-factory-contract": "self.version",
        "viserio/options-resolver": "self.version"
    },
    "provide": {
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.2.0",
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/config": "self.version",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/filesystem": "self.version",
        "viserio/foundation": "self.version",
        "viserio/http-factory": "self.version",
        "viserio/log": "self.version",
        "viserio/phpstan-bridge": "self.version",
        "viserio/view": "self.version"
    },
    "suggest": {
        "filp/whoops": "Enables use of the whoops displayer (^2.2.0).",
        "symfony/console": "Required to use SymfonyConsoleOutput function (^4.2 | ^5.0).",
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/http-factory": "Required to use exception component (self.version).",
        "viserio/view": "Enables use of the view displayer (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Exception\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Exception\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
