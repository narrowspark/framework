{
    "name": "viserio/manager",
    "type": "library",
    "description": "The Viserio Manager package.",
    "keywords": [
        "framework",
        "narrowspark",
        "manager",
        "viserio"
    ],
    "homepage": "https://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/polyfill-ctype": "^1.8.0",
        "viserio/config": "^1.0@dev",
        "viserio/container-contract": "^1.0@dev",
        "viserio/manager-contract": "^1.0@dev"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^4.0.0",
        "narrowspark/testing-helper": "^8.0.2",
        "phpunit/phpunit": "8.2.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Manager\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Manager\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
