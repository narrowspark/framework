{
    "name": "viserio/bus",
    "type": "library",
    "description": "The Viserio Bus package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "command",
        "bus",
        "command bus"
    ],
    "homepage": "http://narrowspark.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "php-di/invoker": "^2.0",
        "psr/container": "^1.0",
        "viserio/contract": "self.version",
        "viserio/pipeline": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "container-interop/service-provider": "^0.4",
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^7.0.0",
        "phpunit/phpunit": "^7.2.0",
        "viserio/container": "self.version",
        "viserio/statical-proxy": "self.version"
    },
    "suggest": {
        "container-interop/service-provider": "Required to use service-provider (^0.4).",
        "viserio/queue": "Required to use queueing dispatcher (self.version).",
        "viserio/statical-proxy": "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Bus\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Bus\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
