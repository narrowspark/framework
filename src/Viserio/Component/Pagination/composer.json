{
    "name": "viserio/pagination",
    "type": "library",
    "description": "The Viserio Pagination package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "pagination",
        "doctrine",
        "array",
        "foundation6",
        "bootstrap4"
    ],
    "homepage": "https://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.4",
        "narrowspark/collection": "^0.1",
        "psr/http-message": "^1.0.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/pagination-contract": "^1.0@dev",
        "viserio/support-contract": "^1.0@dev",
        "viserio/view-contract": "^1.0@dev"
    },
    "provide": {
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.4.0",
        "narrowspark/testing-helper": "^8.0.2",
        "phpunit/phpunit": "8.2.*",
        "psr/container": "^1.0.0",
        "slam/phpstan-extensions": "^4.0.0",
        "viserio/http-factory": "^1.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Pagination\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Pagination\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
