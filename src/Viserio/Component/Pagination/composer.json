{
    "name"       : "viserio/pagination",
    "type"       : "library",
    "description": "The Viserio Pagination package.",
    "keywords"   : ["narrowspark", "viserio", "pagination", "doctrine", "array", "foundation6", "bootstrap4"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : "^7.1",
        "narrowspark/collection"              : "^0.1",
        "psr/http-message"                    : "^1.0",
        "viserio/contracts"                   : "self.version"
    },
    "require-dev": {
        "container-interop/container-interop" : "^1.1",
        "container-interop/service-provider"  : "^0.3",
        "doctrine/dbal"                       : "^2.5",
        "narrowspark/testing-helper"          : "^1.5",
        "phpunit/phpunit"                     : "~5.7",
        "viserio/http-factory"                : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Pagination\\"           : ""
        },
        "exclude-from-classmap"               : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Pagination\\Tests\\"    : "Tests/"
        }
    },
    "provide": {
        "psr/http-message"                : "^1.0",
        "psr/http-message-implementation" : "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "1.0-dev"
        }
    },
    "minimum-stability"                       : "dev",
    "prefer-stable"                           : true
}
