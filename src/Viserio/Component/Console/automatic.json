{
    "configurators": {
        "providers": {
            "Viserio\\Component\\Console\\Container\\Provider\\ConsoleServiceProvider": [
                "global"
            ]
        },
        "config": {
            "global": {
                "viserio": {
                    "console": {
                        "name": "Cerebro",
                        "version": "{APP_VERSION|env|string}"
                    }
                }
            }
        },
        "env": {
            "APP_VERSION": "0.0.0"
        }
    },
    "script-extenders": [
        "Viserio\\Component\\Console\\Automatic\\CerebroScriptExtender"
    ]
}
