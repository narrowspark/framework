{
    "name": "viserio/console",
    "type": "library",
    "description": "The Viserio Console package.",
    "keywords": [
        "framework",
        "narrowspark",
        "cli",
        "command",
        "viserio",
        "cerebro",
        "symfony"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "php-di/invoker": "^2.0",
        "psr/container": "^1.0",
        "symfony/console": "^4.0.6",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/contract": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.0",
        "viserio/container": "self.version",
        "viserio/events": "self.version"
    },
    "suggest": {
        "viserio/statical-proxy": "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "providers": {
                "Viserio\\Component\\Console\\Provider\\ConsoleServiceProvider": [
                    "global"
                ],
                "Viserio\\Component\\Console\\Provider\\LazilyCommandsServiceProvider": [
                    "global"
                ]
            },
            "proxies": {
                "Viserio\\Component\\Console\\Proxy\\Console": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Console\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Console\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
