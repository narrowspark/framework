{
    "name": "viserio/console",
    "type": "library",
    "description": "The Viserio Console package.",
    "keywords": [
        "framework",
        "narrowspark",
        "cli",
        "command",
        "viserio",
        "cerebro",
        "symfony"
    ],
    "homepage": "https://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "php-di/invoker": "^2.0.0",
        "psr/container": "^1.0.0",
        "symfony/console": "^4.3.8 || ^5.0.0",
        "viserio/console-contract": "^1.0@dev",
        "viserio/container-contract": "^1.0@dev",
        "viserio/events-contract": "^1.0@dev",
        "viserio/support": "^1.0@dev"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.4",
        "narrowspark/automatic-common": "^0.7.0",
        "narrowspark/coding-standard": "^3.3.0",
        "narrowspark/testing-helper": "^8.0.2",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^4.0.0",
        "viserio/container": "^1.0@dev",
        "viserio/events": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev"
    },
    "suggest": {
        "viserio/container": "Required to use service-provider (^1.0@dev)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Console\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Console\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
