{
    "name": "viserio/filesystem",
    "type": "library",
    "description": "The Viserio Filesystem package.",
    "keywords": ["narrowspark", "viserio", "aws", "cached", "copy", "dropbox", "files", "flysystem", "ftp", "local", "s3", "sftp", "webdav", "ziparchive"],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "league/flysystem": "^1.0.55",
        "symfony/filesystem": "^4.3.8 || ^5.0.0",
        "symfony/finder": "^4.3.8 || ^5.0.0",
        "viserio/cache-contract": "^1.0@dev",
        "viserio/filesystem-contract": "^1.0@dev",
        "viserio/manager": "^1.0@dev",
        "viserio/options-resolver": "^1.0@dev"
    },
    "require-dev": {
        "league/flysystem-aws-s3-v3": "^1.0.19",
        "league/flysystem-cached-adapter": "^1.0.6",
        "league/flysystem-dropbox": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-vfs": "^1.0",
        "league/flysystem-webdav": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
        "mikey179/vfsstream": "^1.6.8",
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.3.0",
        "narrowspark/testing-helper": "^8.0.2",
        "paragonie/halite": "^4.6.0",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^4.0.0",
        "spatie/flysystem-dropbox": "^1.1.0",
        "viserio/cache": "^1.0@dev",
        "viserio/container": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 adapter (^1.0.19).",
        "league/flysystem-cached-adapter": "Required to use the Flysystem Caching adapter (^1.0.6).",
        "league/flysystem-dropbox": "Required to use the Flysystem Dropbox adapter (^1.0).",
        "league/flysystem-replicate-adapter": "Required to use the Flysystem Replicate adapter (^1.0).",
        "league/flysystem-sftp": "Required to use the Flysystem SFTP adapter (^1.0).",
        "league/flysystem-vfs": "Required to use the Flysystem VFS adapter (^1.0).",
        "league/flysystem-webdav": "Required to use the Flysystem WebDav adapter (^1.0).",
        "league/flysystem-ziparchive": "Required to use the Flysystem ZipArchive adapter (^1.0).",
        "paragonie/halite": "Required to use the encryption for files (^4.6.0).",
        "viserio/container": "Required to use service-provider (^1.0@dev).",
        "ext-inotify": "Required to use INotifyWatcher (>=0.1.6)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Filesystem\\": ""
        },
        "files": [
            "helper.php"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Filesystem\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
