{
    "name": "viserio/filesystem",
    "type": "library",
    "description": "The Viserio Filesystem package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "aws",
        "cached",
        "copy",
        "dropbox",
        "files",
        "flysystem",
        "ftp",
        "local",
        "s3",
        "sftp",
        "rackspace",
        "webdav",
        "ziparchive"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "league/flysystem": "^1.0.44",
        "spatie/macroable": "^1.0",
        "symfony/filesystem": "^4.1.0",
        "symfony/finder": "^4.1.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/contract": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/support": "self.version"
    },
    "require-dev": {
        "container-interop/service-provider": "^0.4",
        "league/flysystem-aws-s3-v3": "^1.0.19",
        "league/flysystem-cached-adapter": "^1.0.6",
        "league/flysystem-dropbox": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-vfs": "^1.0",
        "league/flysystem-webdav": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
        "mikey179/vfsStream": "^1.6",
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^7.0.0",
        "paragonie/halite": "^4.4.2",
        "phpunit/phpunit": "^7.2.0",
        "viserio/cache": "self.version",
        "viserio/container": "self.version"
    },
    "suggest": {
        "container-interop/service-provider": "Required to use service-provider (^0.4).",
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 adapter (^1.0.19).",
        "league/flysystem-cached-adapter": "Required to use the Flysystem Caching adapter (^1.0.6).",
        "league/flysystem-dropbox": "Required to use the Flysystem Dropbox adapter (^1.0).",
        "league/flysystem-rackspace": "Required to use the Flysystem Rackspace adapter (^1.0).",
        "league/flysystem-replicate-adapter": "Required to use the Flysystem Replicate adapter (^1.0).",
        "league/flysystem-sftp": "Required to use the Flysystem SFTP adapter (^1.0).",
        "league/flysystem-vfs": "Required to use the Flysystem VFS adapter (^1.0).",
        "league/flysystem-webdav": "Required to use the Flysystem WebDav adapter (^1.0).",
        "league/flysystem-ziparchive": "Required to use the Flysystem ZipArchive adapter (^1.0).",
        "paragonie/halite": "Required to use the encryption for files (^4.4.2).",
        "viserio/statical-proxy": "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "providers": {
                "Viserio\\Component\\Filesystem\\Provider\\FilesServiceProvider": [
                    "global"
                ],
                "Viserio\\Component\\Filesystem\\Provider\\FilesystemServiceProvider": [
                    "global"
                ]
            },
            "proxies": {
                "Viserio\\Component\\Filesystem\\Proxy\\Files": [
                    "global"
                ],
                "Viserio\\Component\\Filesystem\\Proxy\\Storage": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Filesystem\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Filesystem\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
