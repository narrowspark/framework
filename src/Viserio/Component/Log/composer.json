{
    "name": "viserio/log",
    "type": "library",
    "description": "The Viserio Log package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "monolog",
        "psr",
        "log"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "monolog/monolog": "^1.23.0",
        "viserio/contract": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/log-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.0",
        "viserio/container": "self.version",
        "viserio/events": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/profiler": "self.version"
    },
    "suggest": {
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/events": "Required for log messages in logger (^self.version).",
        "viserio/options-resolver": "Required to use service-provider (self.version).",
        "viserio/statical-proxy": "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "providers": {
                "Viserio\\Component\\Log\\Provider\\LoggerServiceProvider": [
                    "global"
                ],
                "Viserio\\Component\\Log\\Provider\\LoggerDataCollectorServiceProvider": [
                    "local",
                    "testing"
                ]
            },
            "proxies": {
                "Viserio\\Component\\Log\\Proxy\\Log": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Log\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Log\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
