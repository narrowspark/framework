{
    "name": "viserio/pipeline",
    "type": "library",
    "description": "The Viserio Pipeline package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "pipeline"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "php-di/invoker": "^2.0",
        "psr/container": "^1.0",
        "viserio/container-contract": "self.version",
        "viserio/pipeline-contract": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/container": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Pipeline\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Pipeline\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
