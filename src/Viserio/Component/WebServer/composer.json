{
    "name": "viserio/web-server",
    "type": "library",
    "description": "The Viserio Web Server package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "web server",
        "web-server",
        "debug web-server",
        "log web-server",
        "php"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/process": "^4.1.0",
        "viserio/console": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/support": "self.version"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "ext-posix": "*",
        "mockery/mockery": "^1.2.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.2.0",
        "psr/http-message": "^1.0.0",
        "symfony/var-dumper": "^4.1.0",
        "viserio/container": "self.version",
        "viserio/events": "self.version",
        "viserio/foundation": "self.version",
        "viserio/http": "self.version",
        "viserio/monolog-bridge": "self.version"
    },
    "suggest": {
        "ext-pcntl": "Required to use the server start command (*).",
        "ext-posix": "Required to use the server start command (*).",
        "container-interop/service-provider": "Required to use service-provider (^0.4).",
        "psr/http-message": "Required to use the RequestContextProvider class for the var-dump connection (^1.0.0)",
        "symfony/var-dumper": "Required to use the server dump command (^4.1.3).",
        "viserio/foundation": "Required to use the SourceContextProvider class for the var-dump connection (self.version).",
        "viserio/monolog-bridge": "Required to use the server log command (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\WebServer\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\WebServer\\Tests\\": "Tests/"
        },
        "files": [
            "Tests/OverwritePhpFunctions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
