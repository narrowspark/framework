{
    "name"       : "viserio/http-factory",
    "type"       : "library",
    "description": "The Viserio Http Factory package.",
    "keywords"   : ["narrowspark", "viserio", "http-factory", "psr-17"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                          : "^7.1",
        "http-interop/http-factory"                    : "^0.3",
        "viserio/http"                                 : "self.version"
    },
    "require-dev": {
        "mockery/mockery"                              : "^0.9",
        "narrowspark/testing-helper"                   : "^3.0",
        "phpunit/phpunit"                              : "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\HttpFactory\\"        : ""
        },
        "exclude-from-classmap"                        : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\HttpFactory\\Tests\\" : "Tests/"
        }
    },
    "provide": {
        "psr/http-message-implementation"              : "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                               : "1.0-dev"
        }
    },
    "minimum-stability"                                : "dev",
    "prefer-stable"                                    : true
}
