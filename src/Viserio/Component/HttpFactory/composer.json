{
    "name": "viserio/http-factory",
    "type": "library",
    "description": "The Viserio Http Factory package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "http-factory",
        "psr-17"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/http-factory": "^1.0.0",
        "viserio/http": "self.version"
    },
    "provide": {
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "http-interop/http-factory-tests": "^0.5.0",
        "phpunit/phpunit": "^7.5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\HttpFactory\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\HttpFactory\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
