{
    "name": "viserio/parser",
    "type": "library",
    "description": "The Viserio Parsers package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "ini",
        "php",
        "toml",
        "yaml",
        "xml",
        "xliff",
        "xlf",
        "parser",
        "parser",
        "dumper"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-fileinfo": "*",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "symfony/polyfill-xml": "^1.8.0",
        "symfony/var-exporter": "^4.3.5 || ^5.0.0",
        "viserio/parser-contract": "self.version"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.7",
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "symfony/yaml": "^4.3.5 || ^5.0.0",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/phpstan-bridge": "self.version",
        "yosymfony/toml": "^1.0.3"
    },
    "suggest": {
        "symfony/yaml": "Required to use the YAML parser (^4.2 | ^5.0).",
        "viserio/console": "Required to use the YAML and XLIFF lint command (self.version).",
        "viserio/container": "Required to use service-provider (self.version).",
        "yosymfony/toml": "Required to use the TOML parser (^1.0.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Parser\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Parser\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
