{
    "name": "viserio/parser",
    "type": "library",
    "description": "The Viserio Parsers package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "ini",
        "php",
        "toml",
        "yaml",
        "xml",
        "xliff",
        "xlf",
        "parser",
        "parser",
        "dumper"
    ],
    "homepage": "https://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-fileinfo": "*",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "symfony/polyfill-xml": "^1.8.0",
        "symfony/var-exporter": "^4.3.8 || ^5.0.0",
        "viserio/parser-contract": "^1.0@dev"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.8",
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.3.0",
        "narrowspark/testing-helper": "^8.0.2",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^4.0.0",
        "symfony/yaml": "^4.3.8 || ^5.0.0",
        "viserio/console": "^1.0@dev",
        "viserio/container": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev",
        "yosymfony/toml": "^1.0.3"
    },
    "suggest": {
        "symfony/yaml": "Required to use the YAML parser (^4.3.8 | ^5.0).",
        "viserio/console": "Required to use the YAML and XLIFF lint command (^1.0@dev).",
        "viserio/container": "Required to use service-provider (^1.0@dev).",
        "yosymfony/toml": "Required to use the TOML parser (^1.0.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Parser\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Parser\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
