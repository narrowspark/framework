{
    "name": "viserio/parser",
    "type": "library",
    "description": "The Viserio Parsers package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "ini",
        "php",
        "toml",
        "yaml",
        "xml",
        "xliff",
        "xlf",
        "parser",
        "parser",
        "dumper"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-fileinfo": "*",
        "narrowspark/pretty-array": "^1.0",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/contract": "self.version",
        "viserio/support": "self.version"
    },
    "require-dev": {
        "container-interop/service-provider": "^0.4",
        "mikey179/vfsStream": "^1.6",
        "narrowspark/testing-helper": "^7.0.0",
        "phpunit/phpunit": "^7.2.0",
        "symfony/polyfill-xml": "^1.4",
        "symfony/yaml": "^4.1.0",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "yosymfony/toml": "^1.0.3"
    },
    "suggest": {
        "container-interop/service-provider": "Required to use service-provider (^0.4).",
        "symfony/yaml": "Required to use the YAML parser (^4.1.0).",
        "viserio/console": "Required to use the YAML and XLIFF lint command (self.version).",
        "viserio/statical-proxy": "Required to use the proxy instances (self.version).",
        "yosymfony/toml": "Required to use the TOML parser (^1.0.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "automatic": {
            "providers": {
                "Viserio\\Component\\Parser\\Provider\\ParserServiceProvider": [
                    "global"
                ]
            },
            "proxies": {
                "Viserio\\Component\\Parser\\Proxy\\Dumper": [
                    "global"
                ],
                "Viserio\\Component\\Parser\\Proxy\\Parser": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Parser\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Parser\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
