{
    "name": "viserio/support",
    "type": "library",
    "description": "The Viserio Support package.",
    "keywords": [
        "framework",
        "narrowspark",
        "support",
        "viserio",
        "env",
        "manager",
        "dumper",
        "invoker"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/support-contract": "^1.0@dev"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "doctrine/inflector": "^1.3",
        "mikey179/vfsstream": "^1.6.7",
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.2",
        "php-di/invoker": "^2.0",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/container": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev",
        "voku/stringy": "^5.1.0"
    },
    "suggest": {
        "doctrine/inflector": "Required to use the Pluralizer, Str::plural and Str::singular (^1.3).",
        "php-di/invoker": "Required to use the Invoker Class (^1.3).",
        "voku/stringy": "Required to use the full power of Str (^5.1)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Support\\": ""
        },
        "files": [
            "helper.php"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Support\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
