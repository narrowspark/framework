{
    "name"       : "viserio/support",
    "type"       : "library",
    "description": "The Viserio Support package.",
    "keywords"   : ["framework", "narrowspark", "support", "viserio", "env", "manager", "dumper", "invoker"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                      : "^7.2",
        "ext-mbstring"                             : "*",
        "spatie/macroable"                         : "^1.0",
        "viserio/contract"                         : "self.version"
    },
    "require-dev": {
        "danielstjules/stringy"                    : "^3.0",
        "doctrine/inflector"                       : "~1.2",
        "mikey179/vfsStream"                       : "^1.6",
        "mockery/mockery"                          : "^1.0",
        "narrowspark/testing-helper"               : "^4.0",
        "php-di/invoker"                           : "^2.0",
        "phpunit/phpunit"                          : "^6.0",
        "symfony/var-dumper"                       : "~4.0",
        "viserio/config"                           : "self.version",
        "viserio/options-resolver"                 : "self.version"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Viserio\\Component\\Support\\"        : ""
        },
        "exclude-from-classmap"                    : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Support\\Tests\\" : "Tests/"
        }
    },
    "provide": {
        "psr/container-implementation"             : "^1.0"
    },
    "suggest": {
        "danielstjules/stringy"                    : "Required to use the full power of Str (^3.0).",
        "doctrine/inflector"                       : "Required to use the Pluralizer, Str::plural and Str::singular (~1.2).",
        "php-di/invoker"                           : "Required to use the Invoker Class (^1.3).",
        "viserio/options-resolver"                 : "Required to use the AbstractConnectionManager and AbstractManager Class (self.version).",
        "symfony/var-dumper"                       : "Improves the Dumper::dump function (~3.4)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                           : "1.0-dev"
        }
    },
    "minimum-stability"                            : "dev",
    "prefer-stable"                                : true
}
