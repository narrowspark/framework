{
    "name": "viserio/config",
    "type": "library",
    "description": "The Viserio Config package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "config",
        "configuration"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "narrowspark/arr": "^2.1",
        "viserio/config-contract": "self.version",
        "viserio/parser-contract": "self.version"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.7",
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "symfony/var-exporter": "^4.3.5 || ^5.0.0",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/dotenv-bridge": "self.version",
        "viserio/foundation": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/parser": "self.version",
        "viserio/phpstan-bridge": "self.version"
    },
    "suggest": {
        "symfony/var-exporter": "Required to use the config cache and clear command (^4.3 | ^5.0).",
        "viserio/console": "Required to use the config cache and clear command (self.version).",
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/dotenv-bridge": "Required to use EnvParameterProcessor (self.version).",
        "viserio/parser": "Required to import yaml, ini, json, toml and xml config files (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Config\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Config\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
