{
    "name": "viserio/http",
    "type": "library",
    "description": "The Viserio Http package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "http",
        "request",
        "response",
        "psr-7",
        "parse_url",
        "request",
        "response",
        "message",
        "stream",
        "http",
        "uri",
        "url",
        "league",
        "mime-type"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "league/uri": "^5.3",
        "narrowspark/http-message-util": "^0.2.0",
        "narrowspark/http-status": "^4.0",
        "narrowspark/mimetypes":  "^0.1.5",
        "psr/http-message": "^1.0"
    },
    "replace": {
        "symfony/polyfill-mbstring": "*"
    },
    "provide": {
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^6.0",
        "nyholm/nsa": "^1.1",
        "phpunit/phpunit": "^7.0",
        "viserio/support": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Http\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Http\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
