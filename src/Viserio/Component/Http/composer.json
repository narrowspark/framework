{
    "name"       : "viserio/http",
    "type"       : "library",
    "description": "The Viserio Http package.",
    "keywords"   : ["narrowspark", "viserio", "http", "request", "response", "psr-7", "psr-17", "parse_url", "request", "response", "message", "stream", "http", "uri", "url"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : "^7.1",
        "narrowspark/http-status"         : "^2.0",
        "psr/http-message"                : "^1.0",
        "symfony/polyfill-mbstring"       : "^1.0",
        "symfony/polyfill-intl-icu"       : "^1.0",
        "viserio/contracts"               : "self.version"
    },
    "require-dev": {
        "mockery/mockery"                 : "^0.9",
        "narrowspark/testing-helper"      : "^2.0",
        "phpunit/phpunit"                 : "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Http\\"             : ""
        },
        "exclude-from-classmap"           : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Http\\Tests\\"      : "Tests/"
        }
    },
    "provide": {
        "psr/http-message"                : "^1.0",
        "psr/http-message-implementation" : "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
