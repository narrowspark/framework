{
    "name": "viserio/http",
    "type": "library",
    "description": "The Viserio Http package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "http",
        "request",
        "response",
        "psr-7",
        "parse_url",
        "request",
        "response",
        "message",
        "stream",
        "http",
        "uri",
        "url",
        "league",
        "mime-type"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "league/uri-schemes": "^1.2",
        "narrowspark/http-message-util": "^0.3.0",
        "narrowspark/http-status": "^4.1.0",
        "narrowspark/mimetypes": "^1.4.0",
        "psr/http-message": "^1.0.0",
        "viserio/http-contract": "^1.0@dev"
    },
    "provide": {
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "ext-zlib": "*",
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.3.0",
        "narrowspark/testing-helper": "^8.0.2",
        "nyholm/nsa": "^1.1.0",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^4.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Http\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Http\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
