{
    "name"       : "viserio/http",
    "type"       : "library",
    "description": "The Viserio Http package.",
    "keywords"   : ["narrowspark", "viserio", "http", "request", "response", "psr-7", "psr-17", "parse_url", "request", "response", "message", "stream", "http", "uri", "url"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                   : "^7.2",
        "ext-curl"                              : "*",
        "ext-intl"                              : "*",
        "ext-mbstring"                          : "*",
        "fig/http-message-util"                 : "^1.1",
        "league/uri"                            : "^5.3",
        "narrowspark/http-status"               : "^4.0",
        "psr/http-message"                      : "^1.0"
    },
    "require-dev": {
        "narrowspark/testing-helper"            : "^6.0",
        "nyholm/nsa"                            : "^1.1",
        "mockery/mockery"                       : "^1.0",
        "phpunit/phpunit"                       : "^7.0",
        "viserio/support"                       : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Http\\"        : ""
        },
        "exclude-from-classmap"                 : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Http\\Tests\\" : "Tests/"
        }
    },
    "provide": {
        "psr/http-message-implementation"       : "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                        : "1.0-dev"
        }
    },
    "minimum-stability"                         : "dev",
    "prefer-stable"                             : true
}
