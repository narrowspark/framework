{
    "name": "viserio/view",
    "type": "library",
    "description": "The Viserio View package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "view",
        "php"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/container": "^1.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/contract": "self.version",
        "viserio/filesystem": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "erusev/parsedown-extra": "^0.7",
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.0",
        "psr/http-server-middleware": "^1.0",
        "viserio/config": "self.version",
        "viserio/http-factory": "self.version"
    },
    "suggest": {
        "psr/http-server-middleware": "Required to use middleware (^1.0).",
        "viserio/http-factory": "Required to use the Proxy View::createResponseView (^0.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "providers": {
                "Viserio\\Component\\View\\Provider\\ViewServiceProvider": [
                    "global"
                ]
            },
            "proxies": {
                "Viserio\\Component\\View\\Proxy\\View": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\View\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\View\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
