{
    "name": "viserio/cron",
    "type": "library",
    "description": "The Viserio Cron package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "cron"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "cakephp/chronos": "^1.0.4",
        "dragonmantank/cron-expression": "^2.1.0",
        "php-di/invoker": "^2.0.0",
        "psr/cache": "^1.0.1",
        "psr/container": "^1.0.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "symfony/process": "^4.3.8 || ^5.0.0",
        "viserio/container-contract": "^1.0@dev",
        "viserio/cron-contract": "^1.0@dev",
        "viserio/support": "^1.0@dev"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.3.0",
        "narrowspark/testing-helper": "^8.0.2",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^4.0.0",
        "viserio/cache": "^1.0@dev",
        "viserio/console": "^1.0@dev",
        "viserio/container": "^1.0@dev",
        "viserio/foundation": "^1.0@dev",
        "viserio/options-resolver": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev"
    },
    "suggest": {
        "psr/cache": "WithoutOverlapping requires a cache component (^1.0.1).",
        "viserio/console": "Required to use commands (^1.0@dev).",
        "viserio/container": "Required to use service-provider (^1.0@dev).",
        "viserio/options-resolver": "Required to use service-provider and commands (^1.0@dev)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Cron\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Cron\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
