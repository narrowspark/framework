{
    "name": "viserio/cron",
    "type": "library",
    "description": "The Viserio Cron package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "cron"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "cakephp/chronos": "^1.0.4",
        "dragonmantank/cron-expression": "^2.1.0",
        "php-di/invoker": "^2.0",
        "psr/cache": "^1.0.1",
        "psr/container": "^1.0",
        "spatie/macroable": "^1.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "symfony/process": "^4.1.0",
        "viserio/contract": "self.version",
        "viserio/support": "self.version"
    },
    "require-dev": {
        "container-interop/service-provider": "^0.4",
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.2.0",
        "viserio/cache": "self.version",
        "viserio/console": "self.version",
        "viserio/options-resolver": "self.version"
    },
    "suggest": {
        "container-interop/service-provider": "Required to use service-provider (^0.4).",
        "psr/cache": "WithoutOverlapping requires a cache component (^1.0.1).",
        "viserio/console": "Required to use commands (self.version).",
        "viserio/options-resolver": "Required to use service-provider and commands (self.version).",
        "viserio/statical-proxy": "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "providers": {
                "Viserio\\Component\\Cron\\Provider\\ConsoleCommandsServiceProvider": [
                    "global"
                ],
                "Viserio\\Component\\Cron\\Provider\\CronServiceProvider": [
                    "global"
                ]
            },
            "proxies": {
                "Viserio\\Component\\Cron\\Proxy\\Schedule": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Cron\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Cron\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
