{
    "name"       : "viserio/cookie",
    "type"       : "library",
    "description": "The Viserio Cache package.",
    "keywords"   : ["narrowspark", "viserio", "cookie", "RFC6896", "RFC", "psr7"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : "^7.1",
        "cakephp/chronos"                     : "^1.0",
        "psr/http-message"                    : "^1.0",
        "viserio/cotracts"                    : "self.version"
    },
    "require-dev": {
        "container-interop/service-provider"  : "^0.3",
        "mockery/mockery"                     : "^0.9",
        "narrowspark/testing-helper"          : "^2.0",
        "phpunit/phpunit"                     : "~5.7",
        "http-interop/http-middleware"        : "^0.3",
        "viserio/config"                      : "self.version",
        "viserio/container"                   : "self.version",
        "viserio/encrypter"                   : "self.version",
        "viserio/http-factory"                : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Cookie\\"               : ""
        },
        "exclude-from-classmap"               : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Cookie\\Tests\\"        : "Tests/"
        }
    },
    "provide": {
        "psr/http-message"                    : "^1.0",
        "psr/http-message-implementation"     : "^1.0"
    },
    "suggest": {
        "container-interop/service-provider"  : "Required to use service-provider (^0.3).",
        "viserio/encrypter"                   : "Required to use the EncryptCookies Middleware (self.version).",
        "viserio/statical-proxy"              : "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "1.0-dev"
        }
    },
    "minimum-stability"                       : "dev",
    "prefer-stable"                           : true
}
