{
    "name": "viserio/cookie",
    "type": "library",
    "description": "The Viserio Cache package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "cookie",
        "RFC6896",
        "RFC",
        "psr7"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "cakephp/chronos": "^1.0.4",
        "psr/http-message": "^1.0",
        "viserio/cookie-contract": "self.version",
        "viserio/support-contract": "self.version"
    },
    "provide": {
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "paragonie/halite": "^4.6.0",
        "phpunit/phpunit": "8.2.*",
        "psr/http-server-middleware": "^1.0",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/console": "self.version",
        "viserio/container": "self.version",
        "viserio/http-factory": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/phpstan-bridge": "self.version"
    },
    "suggest": {
        "psr/http-server-middleware": "Required to use middleware (^1.0.0).",
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/options-resolver": "Required to use service-provider (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Cookie\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Cookie\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
