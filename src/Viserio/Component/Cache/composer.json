{
    "name"       : "viserio/cache",
    "type"       : "library",
    "description": "The Viserio Cache package.",
    "keywords"   : [
        "narrowspark",
        "viserio",
        "apc",
        "apcu",
        "redis",
        "mongodb",
        "mongo",
        "sqlite",
        "mysql",
        "postgres",
        "pgsql",
        "memcached",
        "memcache",
        "psrCache",
        "psr-6",
        "psr-cache",
        "filesystem",
        "cache",
        "caching"
    ],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                    : "^7.2",
        "cache/array-adapter"                    : "^1.0",
        "cache/void-adapter"                     : "^1.0",
        "psr/container"                          : "^1.0",
        "viserio/contract"                       : "self.version",
        "viserio/options-resolver"               : "self.version",
        "viserio/support"                        : "self.version"
    },
    "require-dev": {
        "cache/filesystem-adapter"               : "^1.0",
        "cache/namespaced-cache"                 : "^1.0",
        "container-interop/service-provider"     : "^0.4",
        "mockery/mockery"                        : "^1.0",
        "narrowspark/testing-helper"             : "^4.0",
        "phpunit/phpunit"                        : "^6.0",
        "viserio/config"                         : "self.version",
        "viserio/container"                      : "self.version"
    },
    "provide": {
        "psr/cache-implementation"               : "^1.0",
        "psr/simple-cache-implementation"        : "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Cache\\"        : ""
        },
        "exclude-from-classmap"                  : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Cache\\Tests\\" : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                         : "1.0-dev"
        }
    },
    "suggest": {
        "cache/array-adapter"                    : "Required to use the Array cache (^1.0)",
        "cache/encryption-cache"                 : "Required to use the Encryption cache (^1.0)",
        "cache/filesystem-adapter"               : "Required to use the Filesystem cache (^1.0).",
        "cache/memcache-adapter"                 : "Required to use the Memcache cache (^1.0).",
        "cache/memcached-adapter"                : "Required to use the Memcached cache (^1.0).",
        "cache/namespaced-cache"                 : "Required to use the Namespaced cache (^1.0).",
        "cache/mongodb-adapter"                  : "Required to use the Mongodb cache (^1.0).",
        "cache/predis-adapter"                   : "Required to use the Predis cache (^1.0).",
        "container-interop/service-provider"     : "Required to use service-provider (^0.4).",
        "viserio/statical-proxy"                 : "Required to use the proxy instances (self.version)."
    },
    "minimum-stability"                          : "dev",
    "prefer-stable"                              : true
}
