{
    "name": "viserio/cache",
    "type": "library",
    "description": "The Viserio Cache package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "apc",
        "apcu",
        "redis",
        "mongodb",
        "mongo",
        "sqlite",
        "mysql",
        "postgres",
        "pgsql",
        "memcached",
        "memcache",
        "psrCache",
        "psr-6",
        "psr-cache",
        "filesystem",
        "cache",
        "caching"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "cache/array-adapter": "^1.0.0",
        "cache/void-adapter": "^1.0.0",
        "psr/container": "^1.0.0",
        "viserio/cache-contract": "self.version",
        "viserio/manager": "self.version",
        "viserio/options-resolver": "self.version"
    },
    "provide": {
        "psr/cache-implementation": "^1.0.0",
        "psr/simple-cache-implementation": "^1.0.0"
    },
    "require-dev": {
        "cache/filesystem-adapter": "^1.0.0",
        "cache/namespaced-cache": "^1.0.0",
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/container": "self.version",
        "viserio/phpstan-bridge": "self.version"
    },
    "suggest": {
        "cache/array-adapter": "Required to use the Array cache (^1.0.0)",
        "cache/filesystem-adapter": "Required to use the Filesystem cache (^1.0.0).",
        "cache/memcache-adapter": "Required to use the Memcache cache (^1.0.0).",
        "cache/memcached-adapter": "Required to use the Memcached cache (^1.0.0).",
        "cache/mongodb-adapter": "Required to use the Mongodb cache (^1.0.0).",
        "cache/namespaced-cache": "Required to use the Namespaced cache (^1.0.0).",
        "cache/predis-adapter": "Required to use the Predis cache (^1.0.0).",
        "viserio/container": "Required to use service-provider (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Cache\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Cache\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
