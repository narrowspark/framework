{
    "name": "viserio/routing",
    "type": "library",
    "description": "The Viserio Routing package.",
    "keywords": [
        "viserio",
        "narrowspark",
        "route",
        "dispatcher",
        "router",
        "routing",
        "middleware",
        "psr-7",
        "psr-15"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "narrowspark/http-status": "^4.1.0",
        "php-di/invoker": "^2.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "symfony/polyfill-ctype": "^1.8.0",
        "symfony/var-exporter": "^4.3.0 || ^5.0",
        "viserio/events-contract": "^1.0@dev",
        "viserio/routing-contract": "^1.0@dev",
        "viserio/support": "^1.0@dev"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-server-middleware-implementation": "^1.0"
    },
    "require-dev": {
        "doctrine/inflector": "^1.3",
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.2",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/console": "^1.0@dev",
        "viserio/container": "^1.0@dev",
        "viserio/events": "^1.0@dev",
        "viserio/http": "^1.0@dev",
        "viserio/http-factory": "^1.0@dev",
        "viserio/options-resolver": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev",
        "viserio/pipeline": "^1.0@dev",
        "viserio/profiler": "^1.0@dev"
    },
    "suggest": {
        "doctrine/inflector": "Required to use the Resource routes (^1.3.0).",
        "psr/http-factory": "Required to use the UrlGenerator (^1.0.0).",
        "viserio/container": "Required to use service-provider (^1.0@dev).",
        "viserio/pipeline": "Required to use psr-15 middleware (^1.0@dev).",
        "viserio/profiler": "Required to use the data collector (^1.0@dev)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Routing\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Tests\\Routing\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
