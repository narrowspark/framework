{
    "name": "viserio/session",
    "type": "library",
    "description": "The Viserio Session package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "cookie",
        "encrypter",
        "session",
        "csrf",
        "secure",
        "flash"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "cache/session-handler": "^1.0",
        "cakephp/chronos": "^1.0.4",
        "paragonie/halite": "^4.4.2",
        "viserio/contract": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "cache/filesystem-adapter": "^1.0",
        "container-interop/service-provider": "^0.4",
        "mikey179/vfsStream": "^1.6",
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.0",
        "psr/http-server-middleware": "^1.0",
        "viserio/cache": "self.version",
        "viserio/config": "self.version",
        "viserio/http-factory": "self.version"
    },
    "suggest": {
        "cache/session-handler": "Required to use the CacheBasedSessionHandler (^0.2).",
        "container-interop/service-provider": "Required to use service-provider (^0.4).",
        "psr/http-server-middleware": "Required to use middleware (^1.0).",
        "viserio/cache": "Required to use the CookieSessionHandler (self.version).",
        "viserio/cookie": "Required to use the CookieSessionHandler and StartSessionMiddleware class (self.version).",
        "viserio/statical-proxy": "Required to use the proxy instances (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "providers": {
                "Viserio\\Component\\Session\\Provider\\SessionServiceProvider": [
                    "global"
                ]
            },
            "proxies": {
                "Viserio\\Component\\Session\\Proxy\\Session": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Session\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Session\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
