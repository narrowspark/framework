{
    "name": "viserio/session",
    "type": "library",
    "description": "The Viserio Session package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "cookie",
        "encrypter",
        "session",
        "csrf",
        "secure",
        "flash"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "cakephp/chronos": "^1.0.4",
        "paragonie/halite": "^4.4.2",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/cache-contract": "self.version",
        "viserio/cookie-contract": "self.version",
        "viserio/manager": "self.version",
        "viserio/options-resolver": "self.version",
        "viserio/session-contract": "self.version",
        "viserio/support": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "ext-pdo": "*",
        "cache/filesystem-adapter": "^1.0",
        "cache/session-handler": "^1.0",
        "mikey179/vfsstream": "^1.6.7",
        "mockery/mockery": "^1.2.3",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.1",
        "phpunit/phpunit": "8.2.*",
        "psr/http-server-middleware": "^1.0",
        "slam/phpstan-extensions": "^3.6.1",
        "viserio/cache": "self.version",
        "viserio/container": "self.version",
        "viserio/cookie": "self.version",
        "viserio/http": "self.version",
        "viserio/http-factory": "self.version",
        "viserio/phpstan-bridge": "self.version",
        "voku/stringy": "^5.2.0"
    },
    "suggest": {
        "cache/session-handler": "Required to use the CacheBasedSessionHandler (^0.2).",
        "psr/http-server-middleware": "Required to use middleware (^1.0).",
        "viserio/cache": "Required to use the CookieSessionHandler (self.version).",
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/cookie": "Required to use the CookieSessionHandler and StartSessionMiddleware class (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\Session\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\Session\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
