{
    "name": "viserio/statical-proxy",
    "type": "library",
    "description": "The Viserio Statical Proxy package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "static",
        "facade",
        "static proxy",
        "static interface",
        "statical",
        "alias",
        "real-time proxy"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/container": "^1.0",
        "symfony/polyfill-mbstring": "^1.8.0",
        "viserio/contract": "self.version"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.0",
        "symfony/filesystem": "^4.0.6",
        "viserio/container": "self.version",
        "viserio/options-resolver": "self.version"
    },
    "suggest": {
        "viserio/container": "Required to use service-provider (self.version).",
        "viserio/options-resolver": "Required to use service-provider (self.version)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "discovery": {
            "providers": {
                "Viserio\\Component\\StaticalProxy\\Provider\\AliasLoaderServiceProvider": [
                    "global"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\StaticalProxy\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\StaticalProxy\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
