{
    "name"       : "viserio/options-resolver",
    "type"       : "library",
    "description": "The Viserio Pagination package.",
    "keywords"   : ["narrowspark", "viserio", "options", "option", "component", "resolver"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                   : "^7.1",
        "container-interop/container-interop"   : "^1.1",
        "viserio/contracts"                     : "self.version"
    },
    "require-dev": {
        "container-interop/service-provider"    : "^0.3",
        "narrowspark/testing-helper"            : "^1.5",
        "phpunit/phpunit"                       : "^6.0",
        "viserio/config"                        : "self.version",
        "viserio/container"                     : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\OptionsResolver\\"        : ""
        },
        "exclude-from-classmap"                 : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\OptionsResolver\\Tests\\" : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                        : "1.0-dev"
        }
    },
    "minimum-stability"                         : "dev",
    "prefer-stable"                             : true
}
