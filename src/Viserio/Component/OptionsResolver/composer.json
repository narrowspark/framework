{
    "name": "viserio/options-resolver",
    "type": "library",
    "description": "The Viserio Options Resolver package.",
    "keywords": [
        "narrowspark",
        "viserio",
        "options",
        "option",
        "component",
        "resolver",
        "configuration"
    ],
    "homepage": "http://github.com/narrowspark/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        },
        {
            "name": "Narrowspark Community",
            "homepage": "https://narrowspark.com/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-tokenizer": "*",
        "viserio/options-resolver-contract": "^1.0@dev"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.7",
        "mockery/mockery": "^1.2.4",
        "narrowspark/coding-standard": "^3.1.0",
        "narrowspark/testing-helper": "^8.0.2",
        "phpunit/phpunit": "8.2.*",
        "slam/phpstan-extensions": "^3.6.1",
        "symfony/var-exporter": "^4.3.5 || ^5.0.0",
        "viserio/console": "^1.0@dev",
        "viserio/container": "^1.0@dev",
        "viserio/parser": "^1.0@dev",
        "viserio/phpstan-bridge": "^1.0@dev",
        "viserio/support": "^1.0@dev"
    },
    "suggest": {
        "symfony/var-exporter": "Required to use the option dump command (^4.3 | ^5.0).",
        "viserio/console": "Required to use the option dump command (^1.0@dev)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Component\\OptionsResolver\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Component\\OptionsResolver\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    }
}
