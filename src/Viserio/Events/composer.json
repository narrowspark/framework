{
    "name"       : "viserio/events",
    "type"       : "library",
    "description": "The Viserio Events package.",
    "keywords"   : ["narrowspark", "viserio", "events", "symfony", "event-loop", "asynchronous"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/events",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                : "^5.6|^7.0",
        "viserio/cotracts"                   : "self.version",
        "viserio/container"                  : "self.version",
        "container-interop/container-interop": "~1.0",
        "symfony/event-dispatcher"           : "~2.8.*|~3.0.*"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config"    : "~1.1.5",
        "narrowspark/testing-helper"         : "~1.5",
        "phpunit/phpunit"                    : "~5.1.3"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Events\\"              : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Events\\Tests\\"       : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                     : "1.0-dev"
        }
    },
    "suggest": {
        "ext-libevent"                       : "Allows for use of a more performant event-loop implementation. (>=0.1)",
        "ext-libev"                          : "Allows for use of a more performant event-loop implementation. (~1.0)",
        "ext-event"                          : "Allows for use of a more performant event-loop implementation. (*)"
    },
    "minimum-stability"                      : "dev",
    "prefer-stable"                          : true
}
