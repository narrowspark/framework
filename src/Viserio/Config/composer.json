{
    "name"       : "viserio/config",
    "type"       : "library",
    "description": "The Viserio Config package.",
    "keywords"   : ["narrowspark", "viserio", "config", "configuration"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : "7.0.0 - 7.0.5 || ^7.0.7",
        "narrowspark/arr"                 : "^1.1",
        "viserio/cotracts"                : "self.version"
    },
    "require-dev": {
        "phpunit/phpunit"                 : "^5.1",
        "mikey179/vfsStream"              : "^1.6",
        "narrowspark/testing-helper"      : "^1.5",
        "viserio/parsers"                 : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Config\\"           : ""
        },
        "exclude-from-classmap"           : ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Config\\Tests\\"    : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "suggest": {
        "viserio/parsers"                 : "Required to use FileLoader class (self.version)."
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
