{
    "name"       : "viserio/filesystem",
    "type"       : "library",
    "description": "The Viserio Filesystem package.",
    "keywords"   : ["narrowspark", "viserio", "filesystem", "aws", "sftp", "ftp", "ZipArchive", "rackspace"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/filesystem",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                              : ">=5.6.4",
        "viserio/contracts"                : "self.version",
        "viserio/support"                  : "self.version",
        "symfony/filesystem"               : "~2.8|~3.0"
    },
    "require-dev": {
        "league/flysystem"                 : "~1.0.3",
        "league/flysystem-aws-s3-v3"       : "~1.0",
        "league/flysystem-rackspace"       : "~1.0",
        "league/flysystem-sftp"            : "~1.0",
        "league/flysystem-ziparchive"      : "~1.0",
        "narrowspark/php-cs-fixer-config"  : "~1.1.5",
        "narrowspark/testing-helper"       : "~1.5",
        "phpunit/phpunit"                  : "~5.1.3",
        "mikey179/vfsStream"               : "~1.6"
    },
    "suggest": {
        "league/flysystem"                 : "Required to use the Flysystem local and FTP drivers (~1.3).",
        "league/flysystem-aws-s3-v3"       : "Required to use the Flysystem S3 driver (~1.0).",
        "league/flysystem-rackspace"       : "Required to use the Flysystem rackspace adapter.",
        "league/flysystem-sftp"            : "Required to use the Flysystem sftp adapter.",
        "league/flysystem-ziparchive"      : "Required to use the Flysystem zipArchive adapter."
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Filesystem\\"        : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Filesystem\\Tests\\" : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                   : "1.0-dev"
        }
    },
    "minimum-stability"                    : "dev",
    "prefer-stable"                        : true
}
