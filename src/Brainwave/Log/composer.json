{
    "name"       : "brainwave/log",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["narrowspark", "brainwave", "monolog", "psr", "log"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/n-brainwave/log",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                        : ">=5.6",
        "brainwave/cotracts"         : "self.version",
        "monolog/monolog"            : "~1.13.1",
        "psr/log"                    : "~1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer"        : "~1.10.1",
        "phpunit/phpunit"            : "~5.0.6"
    },
    "autoload": {
        "psr-4": {
            "Brainwave\\Log\\"       : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brainwave\\Log\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"            : "0.10.0-dev"
        }
    },
    "minimum-stability"             : "dev",
    "prefer-stable"                 : true
}
