{
    "name"       : "brainwave/support",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["framework", "narrowspark", "support", "brainwave", "env", "manager", "dumper"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                : ">=5.5.9",
        "ext-mbstring"                       : "*",
        "brainwave/contracts"                : "self.version",
        "container-interop/container-interop": "~1.0",
        "danielstjules/stringy"              : "~1.9",
        "ircmaxell/random-lib"               : "~1.1"
    },
    "require-dev": {
        "mockery/mockery"                    : "~0.9.3"
    },
    "autoload": {
        "psr-4": {
            "Brainwave\\Support\\"           : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brainwave\\Support\\Tests\\"    : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                     : "0.10.0-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper"                 : "Required to use the Helper::dd() (~2.7).",
        "vlucas/phpdotenv"                   : "Required to use the Helper::env() (~1.1)."
    },
    "minimum-stability"                      : "dev",
    "prefer-stable"                          : true
}
