{
    "name"       : "brainwave/translator",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["narrowspark", "brainwave", "translator"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/n-brainwave/translator",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                               : ">=5.5.9",
        "brainwave/contracts"               : "self.version",
        "brainwave/filesystem"              : "self.version"
    },
    "require-dev": {
        "brainwave/cache"                   : "self.version",
        "symfony/event-dispatcher"          : "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Brainwave\\Translator\\"       : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brainwave\\Translator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                    : "0.10.0-dev"
        }
    },
    "minimum-stability"                     : "dev",
    "prefer-stable"                         : true
}
