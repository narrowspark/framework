{
    "name"       : "brainwave/http",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["narrowspark", "brainwave", "http", "request", "response", "stack", "kernel"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/n-brainwave/http",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                          : ">=5.5.9",
        "brainwave/container"          : "self.version",
        "brainwave/cookie"             : "self.version",
        "brainwave/contracts"          : "self.version",
        "brainwave/session"            : "self.version",
        "stack/builder"                : "~1.0.3",
        "symfony/http-foundation"      : "~2.6.9",
        "symfony/http-kernel"          : "~2.6.9"
    },
    "autoload": {
        "psr-4": {
            "Brainwave\\Http\\"        : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brainwave\\Http\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"              : "0.10.0-dev"
        }
    },
    "minimum-stability"               : "dev",
    "prefer-stable"                   : true
}
